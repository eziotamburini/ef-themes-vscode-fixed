{
  "name": "Ef-Trio-Light",
  "$schema": "vscode://schemas/color-theme",
  "type": "light",
  "colors": {
    "activityBar.background": "#f8f5ff",
    "activityBar.foreground": "#4f3363",
    "activityBar.inactiveForeground": "#786e7499",
    "activityBarBadge.background": "#ddb4ff",
    "activityBarBadge.foreground": "#241f48",
    "badge.background": "#ddb4ff",
    "badge.foreground": "#241f48",
    "breadcrumb.activeSelectionForeground": "#375cd6",
    "breadcrumb.background": "#f8f5ff",
    "breadcrumb.focusForeground": "#375cd6",
    "breadcrumb.foreground": "#786e74",
    "breadcrumbPicker.background": "#e3e0e9",
    "button.background": "#ddb4ff",
    "button.foreground": "#241f48",
    "button.hoverBackground": "#ad45ba33",
    "checkbox.border": "#c6bac5",
    "debugToolBar.background": "#f8f5ff",
    "diffEditor.insertedTextBackground": "#05780033",
    "dropdown.background": "#f8f5ff",
    "dropdown.border": "#e3e0e9",
    "editor.background": "#f8f5ff",
    "editor.foreground": "#4f3363",
    "editor.findMatchHighlightBackground": "#a45f2233",
    "editor.lineHighlightBackground": "#cfe6ff",
    "editor.selectionBackground": "#eed0ff",
    "editor.selectionForeground": "#000000",
    "editor.inactiveSelectionBackground": "#eed0ff80",
    "editor.selectionHighlightBackground": "#eed0ff40",
    "editorCursor.foreground": "#4f45ff",
    "editorCursor.background": "#f8f5ff",
    "editorError.foreground": "#d03033",
    "editorGroup.border": "#e3e0e9",
    "editorGroupHeader.tabsBackground": "#efeef7",
    "editorGroupHeader.tabsBorder": "#e3e0e9",
    "editorHoverWidget.background": "#e3e0e9",
    "editorHoverWidget.border": "#c6bac5",
    "editorIndentGuide.background": "#ebe7f1",
    "editorLineNumber.foreground": "#786e74",
    "editorLineNumber.activeForeground": "#000000",
    "editorLink.activeForeground": "#1f6fbf",
    "editorRuler.foreground": "#c6bac5",
    "editorSuggestWidget.background": "#e3e0e9",
    "editorSuggestWidget.border": "#ddb4ff",
    "editorSuggestWidget.selectedBackground": "#ebe7f1",
    "editorSuggestWidget.selectedForeground": "#4f3363",
    "editorWhitespace.foreground": "#c6bac5",
    "editorWidget.background": "#e3e0e9",
    "editorWidget.foreground": "#241f48",
    "extensionButton.prominentBackground": "#ddb4ff33",
    "gitDecoration.addedResourceForeground": "#005000",
    "gitDecoration.modifiedResourceForeground": "#553d00",
    "gitDecoration.deletedResourceForeground": "#8f1313",
    "input.background": "#c3c0c9",
    "input.border": "#ddb4ff",
    "input.foreground": "#4f3363",
    "inputOption.activeBorder": "#1f6fbf",
    "list.activeSelectionBackground": "#c3c0c9",
    "list.activeSelectionForeground": "#4f3363",
    "list.dropBackground": "#eed0ff",
    "list.errorForeground": "#c3303a",
    "list.focusBackground": "#c3c0c9",
    "list.highlightForeground": "#ad45ba",
    "list.hoverBackground": "#e3e0e9",
    "list.inactiveSelectionBackground": "#efeef7",
    "list.inactiveSelectionForeground": "#4f3363",
    "mergeEditor.change.word.background": "#e3e0e9",
    "mergeEditor.conflict.handled.minimapOverViewRuler": "#a45f22",
    "mergeEditor.conflict.unhandled.minimapOverViewRuler": "#1f6fbf",
    "notifications.background": "#ddb4ff",
    "notifications.foreground": "#241f48",
    "notifications.border": "#ddb4ff",
    "peekView.border": "#a45f22",
    "peekViewEditor.background": "#e3e0e9",
    "peekViewEditor.matchHighlightBackground": "#cfe6ff",
    "peekViewResult.background": "#e3e0e9",
    "peekViewTitle.background": "#e3e0e9",
    "scrollbarSlider.activeBackground": "#c3c0c9bb",
    "scrollbarSlider.background": "#e3e0e999",
    "scrollbarSlider.hoverBackground": "#ebe7f1cc",
    "sideBar.background": "#f8f5ff",
    "sideBar.foreground": "#4f3363",
    "sideBar.border": "#e3e0e9",
    "sideBarSectionHeader.background": "#ebe7f1",
    "sideBarSectionHeader.foreground": "#4f3363",
    "sideBarTitle.foreground": "#ad45ba",
    "statusBar.background": "#ddb4ff",
    "statusBar.debuggingBackground": "#a45f22",
    "statusBar.foreground": "#241f48",
    "statusBar.noFolderBackground": "#ddb4ff",
    "statusBarItem.hoverBackground": "#ad45ba33",
    "tab.activeBackground": "#f8f5ff",
    "tab.activeBorder": "#1f6fbf",
    "tab.border": "#efeef7",
    "tab.hoverBackground": "#e3e0e9",
    "tab.inactiveBackground": "#efeef7",
    "tab.inactiveForeground": "#786e74",
    "titleBar.activeBackground": "#f8f5ff",
    "titleBar.activeForeground": "#4f3363",
    "titleBar.inactiveBackground": "#f8f5ff",
    "titleBar.inactiveForeground": "#786e74",
    "tree.indentGuidesStroke": "#c6bac5",
    "terminal.ansiWhite": "#4f3363",
    "terminal.ansiBlack": "#ebe7f1",
    "terminal.ansiBlue": "#375cd6",
    "terminal.ansiCyan": "#1f6fbf",
    "terminal.ansiGreen": "#057800",
    "terminal.ansiMagenta": "#ad45ba",
    "terminal.ansiRed": "#c3303a",
    "terminal.ansiYellow": "#a45f22",
    "terminal.ansiBrightWhite": "#786e74",
    "terminal.ansiBrightBlack": "#c3c0c9",
    "terminal.ansiBrightBlue": "#065fbf",
    "terminal.ansiBrightCyan": "#0f7a9d",
    "terminal.ansiBrightGreen": "#007f6f",
    "terminal.ansiBrightMagenta": "#705ae3",
    "terminal.ansiBrightRed": "#c01f5f",
    "terminal.ansiBrightYellow": "#b65050"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#a05b5f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#007f6f"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#c6bac5"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#c035aa"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#c035aa"
      }
    },
    {
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#705ae3"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#3f6faf"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#3f6faf"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#5165e4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.class",
        "support.type.primitive",
        "storage.type.type"
      ],
      "settings": {
        "foreground": "#0f7a9d"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#ad45ba",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage"
      ],
      "settings": {
        "foreground": "#ad45ba",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#786e74"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#4f3363"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#4f3363",
        "background": "#ffdfe6"
      }
    },
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#1f6fbf",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#a24568",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.tag",
        "entity.name.tag",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#375cd6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#0f7a9d"
      }
    },
    {
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#5165e4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#c01f5f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function.lisp",
      "settings": {
        "foreground": "#5165e4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.lisp",
      "settings": {
        "foreground": "#ad45ba",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language.lisp",
      "settings": {
        "foreground": "#c035aa",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type.function.lisp",
      "settings": {
        "foreground": "#a45f22",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "rx-escape",
      "settings": {
        "foreground": "#0f7a9d"
      }
    },
    {
      "scope": "rx-construct",
      "settings": {
        "foreground": "#c3303a"
      }
    },
    {
      "name": "Line number active",
      "scope": "editorLineNumber.activeForeground",
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    }
  ]
}
