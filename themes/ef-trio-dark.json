{
  "name": "Ef-Trio-Dark",
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "activityBar.background": "#160f0f",
    "activityBar.foreground": "#d8cfd5",
    "activityBar.inactiveForeground": "#90889099",
    "activityBarBadge.background": "#6a294f",
    "activityBarBadge.foreground": "#ffdfdf",
    "badge.background": "#6a294f",
    "badge.foreground": "#ffdfdf",
    "breadcrumb.activeSelectionForeground": "#7fa5f6",
    "breadcrumb.background": "#160f0f",
    "breadcrumb.focusForeground": "#7fa5f6",
    "breadcrumb.foreground": "#908890",
    "breadcrumbPicker.background": "#362f35",
    "button.background": "#6a294f",
    "button.foreground": "#ffdfdf",
    "button.hoverBackground": "#d37faf33",
    "checkbox.border": "#605760",
    "debugToolBar.background": "#160f0f",
    "diffEditor.insertedTextBackground": "#60b44433",
    "dropdown.background": "#160f0f",
    "dropdown.border": "#362f35",
    "editor.background": "#160f0f",
    "editor.foreground": "#d8cfd5",
    "editor.findMatchHighlightBackground": "#d4a05233",
    "editor.lineHighlightBackground": "#34223f",
    "editor.selectionBackground": "#16304f",
    "editor.selectionForeground": "#ffffff",
    "editor.inactiveSelectionBackground": "#16304f80",
    "editor.selectionHighlightBackground": "#16304f40",
    "editorCursor.foreground": "#ff99ff",
    "editorCursor.background": "#160f0f",
    "editorError.foreground": "#ff7560",
    "editorGroup.border": "#362f35",
    "editorGroupHeader.tabsBackground": "#1c1719",
    "editorGroupHeader.tabsBorder": "#362f35",
    "editorHoverWidget.background": "#362f35",
    "editorHoverWidget.border": "#605760",
    "editorIndentGuide.background": "#2a2228",
    "editorLineNumber.foreground": "#908890",
    "editorLineNumber.activeForeground": "#ffffff",
    "editorLink.activeForeground": "#8fbaff",
    "editorRuler.foreground": "#605760",
    "editorSuggestWidget.background": "#362f35",
    "editorSuggestWidget.border": "#6a294f",
    "editorSuggestWidget.selectedBackground": "#2a2228",
    "editorSuggestWidget.selectedForeground": "#d8cfd5",
    "editorWhitespace.foreground": "#605760",
    "editorWidget.background": "#362f35",
    "editorWidget.foreground": "#ffdfdf",
    "extensionButton.prominentBackground": "#6a294f33",
    "gitDecoration.addedResourceForeground": "#a0e0a0",
    "gitDecoration.modifiedResourceForeground": "#efef80",
    "gitDecoration.deletedResourceForeground": "#ffbfbf",
    "input.background": "#564f55",
    "input.border": "#6a294f",
    "input.foreground": "#d8cfd5",
    "inputOption.activeBorder": "#8fbaff",
    "list.activeSelectionBackground": "#564f55",
    "list.activeSelectionForeground": "#d8cfd5",
    "list.dropBackground": "#16304f",
    "list.errorForeground": "#f48359",
    "list.focusBackground": "#564f55",
    "list.highlightForeground": "#d37faf",
    "list.hoverBackground": "#2a2228",
    "list.inactiveSelectionBackground": "#1c1719",
    "list.inactiveSelectionForeground": "#d8cfd5",
    "mergeEditor.change.word.background": "#2a2228",
    "mergeEditor.conflict.handled.minimapOverViewRuler": "#d37faf",
    "mergeEditor.conflict.unhandled.minimapOverViewRuler": "#8fbaff",
    "notifications.background": "#6a294f",
    "notifications.foreground": "#ffdfdf",
    "notifications.border": "#6a294f",
    "peekView.border": "#d37faf",
    "peekViewEditor.background": "#2a2228",
    "peekViewEditor.matchHighlightBackground": "#34223f",
    "peekViewResult.background": "#362f35",
    "peekViewTitle.background": "#2a2228",
    "scrollbarSlider.activeBackground": "#362f35bb",
    "scrollbarSlider.background": "#2a222899",
    "scrollbarSlider.hoverBackground": "#362f35cc",
    "sideBar.background": "#160f0f",
    "sideBar.foreground": "#d8cfd5",
    "sideBar.border": "#362f35",
    "sideBarSectionHeader.background": "#2a2228",
    "sideBarSectionHeader.foreground": "#d8cfd5",
    "sideBarTitle.foreground": "#d37faf",
    "statusBar.background": "#6a294f",
    "statusBar.debuggingBackground": "#d37faf",
    "statusBar.foreground": "#ffdfdf",
    "statusBar.noFolderBackground": "#6a294f",
    "statusBarItem.hoverBackground": "#564f5599",
    "tab.activeBackground": "#160f0f",
    "tab.activeBorder": "#8fbaff",
    "tab.border": "#1c1719",
    "tab.hoverBackground": "#2a2228",
    "tab.inactiveBackground": "#1c1719",
    "tab.inactiveForeground": "#908890",
    "titleBar.activeBackground": "#160f0f",
    "titleBar.activeForeground": "#d8cfd5",
    "titleBar.inactiveBackground": "#160f0f",
    "titleBar.inactiveForeground": "#908890",
    "tree.indentGuidesStroke": "#605760",
    "terminal.ansiWhite": "#d8cfd5",
    "terminal.ansiBlack": "#2a2228",
    "terminal.ansiBlue": "#7fa5f6",
    "terminal.ansiCyan": "#8fbaff",
    "terminal.ansiGreen": "#60b444",
    "terminal.ansiMagenta": "#d37faf",
    "terminal.ansiRed": "#f48359",
    "terminal.ansiYellow": "#d4a052",
    "terminal.ansiBrightWhite": "#908890",
    "terminal.ansiBrightBlack": "#564f55",
    "terminal.ansiBrightBlue": "#72afff",
    "terminal.ansiBrightCyan": "#8fcfdf",
    "terminal.ansiBrightGreen": "#60bf88",
    "terminal.ansiBrightMagenta": "#a698ef",
    "terminal.ansiBrightRed": "#ff85aa",
    "terminal.ansiBrightYellow": "#ef9680"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#c7a07f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#60bf88"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#605760"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#e772df"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#e772df"
      }
    },
    {
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#a698ef"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#9ac2ff"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#9ac2ff"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#8895ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.class",
        "support.type.primitive",
        "storage.type.type"
      ],
      "settings": {
        "foreground": "#8fcfdf"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#d37faf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage"
      ],
      "settings": {
        "foreground": "#d37faf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#908890"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#d8cfd5"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#d8cfd5",
        "background": "#3a0f06"
      }
    },
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#8fbaff",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#e07a9a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.tag",
        "entity.name.tag",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#7fa5f6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#8fcfdf"
      }
    },
    {
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#8895ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#ff85aa",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function.lisp",
      "settings": {
        "foreground": "#8895ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.lisp",
      "settings": {
        "foreground": "#d37faf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language.lisp",
      "settings": {
        "foreground": "#e772df",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type.function.lisp",
      "settings": {
        "foreground": "#ef9680",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "rx-escape",
      "settings": {
        "foreground": "#8895ff"
      }
    },
    {
      "scope": "rx-construct",
      "settings": {
        "foreground": "#f48359"
      }
    },
    {
      "name": "Line number active",
      "scope": "editorLineNumber.activeForeground",
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    }
  ]
}
