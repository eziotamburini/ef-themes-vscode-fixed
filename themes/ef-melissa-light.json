{
  "name": "Ef-Melissa-Light",
  "$schema": "vscode://schemas/color-theme",
  "type": "hcLight",
  "colors": {
    "activityBar.background": "#fff6d8",
    "activityBar.foreground": "#484431",
    "activityBar.inactiveForeground": "#68708a99",
    "activityBarBadge.background": "#f3cf72",
    "activityBarBadge.foreground": "#403328",
    "badge.background": "#f3cf72",
    "badge.foreground": "#403328",
    "breadcrumb.activeSelectionForeground": "#946830",
    "breadcrumb.background": "#fff6d8",
    "breadcrumb.focusForeground": "#946830",
    "breadcrumb.foreground": "#68708a",
    "breadcrumbPicker.background": "#f5e9cb",
    "button.background": "#f5e9cb",
    "button.foreground": "#484431",
    "button.hoverBackground": "#00825018",
    "checkbox.border": "#c5baa6",
    "debugToolBar.background": "#fff6d8",
    "diffEditor.insertedTextBackground": "#d7ecb033",
    "dropdown.background": "#fff6d8",
    "dropdown.border": "#f5e9cb",
    "editor.background": "#fff6d8",
    "editor.foreground": "#484431",
    "editor.findMatchHighlightBackground": "#f4ed7e33",
    "editor.lineHighlightBackground": "#fae7b0",
    "editor.selectionBackground": "#f0d4d8",
    "editor.selectionForeground": "#000000",
    "editor.inactiveSelectionBackground": "#f0d4d880",
    "editor.selectionHighlightBackground": "#f0d4d840",
    "editorCursor.foreground": "#a07f00",
    "editorCursor.background": "#fff6d8",
    "editorError.foreground": "#c74400",
    "editorGroup.border": "#f5e9cb",
    "editorGroupHeader.tabsBackground": "#f5e9cb",
    "editorGroupHeader.tabsBorder": "#f5e9cb",
    "editorHoverWidget.background": "#f5e9cb",
    "editorHoverWidget.border": "#c5baa6",
    "editorIndentGuide.background": "#f5e9cb",
    "editorLineNumber.foreground": "#68708a",
    "editorLineNumber.activeForeground": "#000000",
    "editorLink.activeForeground": "#4060a0",
    "editorRuler.foreground": "#c5baa6",
    "editorSuggestWidget.background": "#f5e9cb",
    "editorSuggestWidget.border": "#f3cf72",
    "editorSuggestWidget.selectedBackground": "#f5e9cb",
    "editorSuggestWidget.selectedForeground": "#484431",
    "editorWhitespace.foreground": "#c5baa6",
    "editorWidget.background": "#f5e9cb",
    "extensionButton.prominentBackground": "#f3cf7233",
    "gitDecoration.addedResourceForeground": "#007a0a",
    "gitDecoration.modifiedResourceForeground": "#553d00",
    "gitDecoration.untrackedResourceForeground": "#48443188",
    "input.background": "#c5baa6",
    "input.border": "#f3cf72",
    "input.foreground": "#484431",
    "inputOption.activeBorder": "#4060a0",
    "list.activeSelectionBackground": "#c5baa6",
    "list.activeSelectionForeground": "#484431",
    "list.dropBackground": "#deb4f0",
    "list.errorForeground": "#ba2d2f",
    "list.focusBackground": "#c5baa6",
    "list.highlightForeground": "#5a7400",
    "list.hoverBackground": "#f5e9cb",
    "list.inactiveSelectionBackground": "#f5e9cb",
    "list.inactiveSelectionForeground": "#484431",
    "mergeEditor.change.word.background": "#f5e9cb",
    "mergeEditor.conflict.handled.minimapOverViewRuler": "#5a7400",
    "mergeEditor.conflict.unhandled.minimapOverViewRuler": "#4060a0",
    "notifications.background": "#f3cf72",
    "notifications.foreground": "#403328",
    "notifications.border": "#f3cf72",
    "peekView.border": "#5a7400",
    "peekViewEditor.background": "#f5e9cb",
    "peekViewEditor.matchHighlightBackground": "#fae7b0",
    "peekViewResult.background": "#f5e9cb",
    "peekViewTitle.background": "#f5e9cb",
    "scrollbarSlider.activeBackground": "#c5baa6bb",
    "scrollbarSlider.background": "#f5e9cb99",
    "scrollbarSlider.hoverBackground": "#c5baa6cc",
    "sideBar.background": "#fff6d8",
    "sideBar.foreground": "#484431",
    "sideBarSectionHeader.background": "#f5e9cb",
    "sideBarSectionHeader.foreground": "#484431",
    "sideBarTitle.foreground": "#5a7400",
    "statusBar.background": "#f3cf72",
    "statusBar.debuggingBackground": "#5a7400",
    "statusBar.foreground": "#403328",
    "statusBar.noFolderBackground": "#f3cf72",
    "statusBarItem.hoverBackground": "#c5baa699",
    "tab.activeBackground": "#fff6d8",
    "tab.activeBorder": "#4060a0",
    "tab.border": "#f5e9cb",
    "tab.hoverBackground": "#f5e9cb",
    "tab.inactiveBackground": "#f5e9cb",
    "tab.inactiveForeground": "#68708a",
    "titleBar.activeBackground": "#fff6d8",
    "titleBar.activeForeground": "#484431",
    "titleBar.inactiveBackground": "#fff6d8",
    "titleBar.inactiveForeground": "#68708a",
    "tree.indentGuidesStroke": "#c5baa6",
    "terminal.ansiWhite": "#484431",
    "terminal.ansiBlack": "#f5e9cb",
    "terminal.ansiBlue": "#375cc6",
    "terminal.ansiCyan": "#3f60af",
    "terminal.ansiGreen": "#007a0a",
    "terminal.ansiMagenta": "#aa3e74",
    "terminal.ansiRed": "#ba2d2f",
    "terminal.ansiYellow": "#a26310",
    "terminal.ansiBrightWhite": "#68708a",
    "terminal.ansiBrightBlack": "#c5baa6",
    "terminal.ansiBrightBlue": "#5f5fdf",
    "terminal.ansiBrightCyan": "#0f708a",
    "terminal.ansiBrightGreen": "#5a7400",
    "terminal.ansiBrightMagenta": "#6448ca",
    "terminal.ansiBrightRed": "#c74400",
    "terminal.ansiBrightYellow": "#ba5205"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7f6f1a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#c74400"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#007a0a"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#3f60af"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#3f60af"
      }
    },
    {
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#3f60af"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#0f708a"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#0f708a"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#5a7400",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.class",
        "support.type.primitive",
        "storage.type.type"
      ],
      "settings": {
        "foreground": "#008250"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#a26310",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage"
      ],
      "settings": {
        "foreground": "#a26310",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#68708a"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#484431"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#484431",
        "background": "#f8d0b6"
      }
    },
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#a26310",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#6448ca",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.tag",
        "entity.name.tag",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#5a7400",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#008250"
      }
    },
    {
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#5a7400",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#ba5205",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function.lisp",
      "settings": {
        "foreground": "#5a7400",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.lisp",
      "settings": {
        "foreground": "#a26310",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language.lisp",
      "settings": {
        "foreground": "#3f60af",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type.function.lisp",
      "settings": {
        "foreground": "#5a7400",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Line number active",
      "scope": "editorLineNumber.activeForeground",
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    }
  ]
}
