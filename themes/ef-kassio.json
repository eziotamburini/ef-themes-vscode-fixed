{
  "name": "Ef-Kassio",
  "$schema": "vscode://schemas/color-theme",
  "type": "light",
  "colors": {
    "activityBar.background": "#fff7f7",
    "activityBar.foreground": "#201f36",
    "activityBar.inactiveForeground": "#776f7999",
    "activityBarBadge.background": "#e0bfba",
    "activityBarBadge.foreground": "#151515",
    "badge.background": "#e0bfba",
    "badge.foreground": "#151515",
    "breadcrumb.activeSelectionForeground": "#1077ab",
    "breadcrumb.background": "#fff7f7",
    "breadcrumb.focusForeground": "#1077ab",
    "breadcrumb.foreground": "#776f79",
    "breadcrumbPicker.background": "#e0dbdb",
    "button.background": "#e0bfba",
    "button.foreground": "#151515",
    "button.hoverBackground": "#00835818",
    "checkbox.border": "#bab7bc",
    "debugToolBar.background": "#fff7f7",
    "diffEditor.insertedTextBackground": "#c2efcf33",
    "dropdown.background": "#fff7f7",
    "dropdown.border": "#e0dbdb",
    "editor.background": "#fff7f7",
    "editor.foreground": "#201f36",
    "editor.findMatchHighlightBackground": "#fac20033",
    "editor.lineHighlightBackground": "#f2e5d9",
    "editor.selectionBackground": "#dfe4f4",
    "editor.selectionForeground": "#000000",
    "editor.inactiveSelectionBackground": "#dfe4f480",
    "editor.selectionHighlightBackground": "#dfe4f440",
    "editorCursor.foreground": "#d06f30",
    "editorCursor.background": "#fff7f7",
    "editorError.foreground": "#e00033",
    "editorGroup.border": "#e0dbdb",
    "editorGroupHeader.tabsBackground": "#f9eff3",
    "editorGroupHeader.tabsBorder": "#e0dbdb",
    "editorHoverWidget.background": "#e0dbdb",
    "editorHoverWidget.border": "#bab7bc",
    "editorIndentGuide.background": "#efe7e7",
    "editorLineNumber.foreground": "#776f79",
    "editorLineNumber.activeForeground": "#000000",
    "editorLink.activeForeground": "#3c3bbe",
    "editorRuler.foreground": "#bab7bc",
    "editorSuggestWidget.background": "#e0dbdb",
    "editorSuggestWidget.border": "#e0bfba",
    "editorSuggestWidget.selectedBackground": "#efe7e7",
    "editorSuggestWidget.selectedForeground": "#201f36",
    "editorWhitespace.foreground": "#bab7bc",
    "editorWidget.background": "#e0dbdb",
    "extensionButton.prominentBackground": "#e0bfba33",
    "gitDecoration.addedResourceForeground": "#005000",
    "gitDecoration.modifiedResourceForeground": "#553d00",
    "gitDecoration.untrackedResourceForeground": "#201f3688",
    "input.background": "#c0bbbb",
    "input.border": "#e0bfba",
    "input.foreground": "#201f36",
    "inputOption.activeBorder": "#3c3bbe",
    "list.activeSelectionBackground": "#c0bbbb",
    "list.activeSelectionForeground": "#201f36",
    "list.dropBackground": "#dfe4f4",
    "list.errorForeground": "#b00234",
    "list.focusBackground": "#c0bbbb",
    "list.highlightForeground": "#3c3bbe",
    "list.hoverBackground": "#efe7e7",
    "list.inactiveSelectionBackground": "#f9eff3",
    "list.inactiveSelectionForeground": "#201f36",
    "mergeEditor.change.word.background": "#efe7e7",
    "mergeEditor.conflict.handled.minimapOverViewRuler": "#217a3c",
    "mergeEditor.conflict.unhandled.minimapOverViewRuler": "#2f5f9f",
    "notifications.background": "#e0bfba",
    "notifications.foreground": "#151515",
    "notifications.border": "#e0bfba",
    "peekView.border": "#3c3bbe",
    "peekViewEditor.background": "#efe7e7",
    "peekViewEditor.matchHighlightBackground": "#f2e5d9",
    "peekViewResult.background": "#e0dbdb",
    "peekViewTitle.background": "#efe7e7",
    "scrollbarSlider.activeBackground": "#c0bbbbbb",
    "scrollbarSlider.background": "#efe7e799",
    "scrollbarSlider.hoverBackground": "#e0dbdbcc",
    "sideBar.background": "#fff7f7",
    "sideBar.foreground": "#201f36",
    "sideBarSectionHeader.background": "#efe7e7",
    "sideBarSectionHeader.foreground": "#201f36",
    "sideBarTitle.foreground": "#3c3bbe",
    "statusBar.background": "#e0bfba",
    "statusBar.debuggingBackground": "#217a3c",
    "statusBar.foreground": "#151515",
    "statusBar.noFolderBackground": "#e0bfba",
    "statusBarItem.hoverBackground": "#c0bbbb99",
    "tab.activeBackground": "#fff7f7",
    "tab.activeBorder": "#3c3bbe",
    "tab.border": "#f9eff3",
    "tab.hoverBackground": "#efe7e7",
    "tab.inactiveBackground": "#f9eff3",
    "tab.inactiveForeground": "#776f79",
    "titleBar.activeBackground": "#fff7f7",
    "titleBar.activeForeground": "#201f36",
    "titleBar.inactiveBackground": "#fff7f7",
    "titleBar.inactiveForeground": "#776f79",
    "tree.indentGuidesStroke": "#bab7bc",
    "terminal.ansiWhite": "#201f36",
    "terminal.ansiBlack": "#efe7e7",
    "terminal.ansiBlue": "#3c3bbe",
    "terminal.ansiCyan": "#2f5f9f",
    "terminal.ansiGreen": "#217a3c",
    "terminal.ansiMagenta": "#a01f64",
    "terminal.ansiRed": "#b00234",
    "terminal.ansiYellow": "#9a6012",
    "terminal.ansiBrightWhite": "#776f79",
    "terminal.ansiBrightBlack": "#c0bbbb",
    "terminal.ansiBrightBlue": "#065fff",
    "terminal.ansiBrightCyan": "#1077ab",
    "terminal.ansiBrightGreen": "#008358",
    "terminal.ansiBrightMagenta": "#7022bf",
    "terminal.ansiBrightRed": "#d5305f",
    "terminal.ansiBrightYellow": "#a04646"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#506fa0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#a04646"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#1077ab"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#7022bf"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#7022bf"
      }
    },
    {
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#7022bf"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#4250ef"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#4250ef"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#9f248a",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.class",
        "support.type.primitive",
        "storage.type.type"
      ],
      "settings": {
        "foreground": "#b00234"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#3c3bbe",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage"
      ],
      "settings": {
        "foreground": "#3c3bbe",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#776f79"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#201f36"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#201f36",
        "background": "#ffdde5"
      }
    },
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#3c3bbe",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#954f90",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.tag",
        "entity.name.tag",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#3c3bbe",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#b00234"
      }
    },
    {
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#9f248a",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#3f6faf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function.lisp",
      "settings": {
        "foreground": "#9f248a",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.lisp",
      "settings": {
        "foreground": "#3c3bbe",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language.lisp",
      "settings": {
        "foreground": "#7022bf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type.function.lisp",
      "settings": {
        "foreground": "#a04646",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "rx-escape",
      "settings": {
        "foreground": "#1077ab"
      }
    },
    {
      "scope": "rx-construct",
      "settings": {
        "foreground": "#d5305f"
      }
    },
    {
      "name": "Line number active",
      "scope": "editorLineNumber.activeForeground",
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    }
  ]
}
