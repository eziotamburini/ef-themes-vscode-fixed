{
  "name": "Ef-Maris-Light",
  "$schema": "vscode://schemas/color-theme",
  "type": "light",
  "colors": {
    "activityBar.background": "#edf4f8",
    "activityBar.foreground": "#151a27",
    "activityBar.inactiveForeground": "#67647099",
    "activityBarBadge.background": "#a0c2ef",
    "activityBarBadge.foreground": "#142810",
    "badge.background": "#a0c2ef",
    "badge.foreground": "#142810",
    "breadcrumb.activeSelectionForeground": "#1f66af",
    "breadcrumb.background": "#edf4f8",
    "breadcrumb.focusForeground": "#1f66af",
    "breadcrumb.foreground": "#676470",
    "breadcrumbPicker.background": "#cfd8e3",
    "button.background": "#a0c2ef",
    "button.foreground": "#142810",
    "button.hoverBackground": "#00704718",
    "checkbox.border": "#a2a6af",
    "debugToolBar.background": "#edf4f8",
    "diffEditor.insertedTextBackground": "#b0e8ca33",
    "dropdown.background": "#edf4f8",
    "dropdown.border": "#cfd8e3",
    "editor.background": "#edf4f8",
    "editor.foreground": "#151a27",
    "editor.findMatchHighlightBackground": "#efbf0033",
    "editor.lineHighlightBackground": "#dae5f0",
    "editor.selectionBackground": "#c8dcff",
    "editor.selectionForeground": "#000000",
    "editor.inactiveSelectionBackground": "#c8dcff80",
    "editor.selectionHighlightBackground": "#c8dcff40",
    "editorCursor.foreground": "#036f99",
    "editorCursor.background": "#edf4f8",
    "editorError.foreground": "#d00000",
    "editorGroup.border": "#cfd8e3",
    "editorGroupHeader.tabsBackground": "#e6ecf2",
    "editorGroupHeader.tabsBorder": "#cfd8e3",
    "editorHoverWidget.background": "#cfd8e3",
    "editorHoverWidget.border": "#a2a6af",
    "editorIndentGuide.background": "#e0e7ef",
    "editorLineNumber.foreground": "#676470",
    "editorLineNumber.activeForeground": "#000000",
    "editorLink.activeForeground": "#375cc6",
    "editorRuler.foreground": "#a2a6af",
    "editorSuggestWidget.background": "#cfd8e3",
    "editorSuggestWidget.border": "#a0c2ef",
    "editorSuggestWidget.selectedBackground": "#e0e7ef",
    "editorSuggestWidget.selectedForeground": "#151a27",
    "editorWhitespace.foreground": "#a2a6af",
    "editorWidget.background": "#cfd8e3",
    "extensionButton.prominentBackground": "#a0c2ef33",
    "gitDecoration.addedResourceForeground": "#005000",
    "gitDecoration.modifiedResourceForeground": "#553d00",
    "gitDecoration.untrackedResourceForeground": "#151a2788",
    "input.background": "#afb8c3",
    "input.border": "#a0c2ef",
    "input.foreground": "#151a27",
    "inputOption.activeBorder": "#375cc6",
    "list.activeSelectionBackground": "#afb8c3",
    "list.activeSelectionForeground": "#151a27",
    "list.dropBackground": "#c8dcff",
    "list.errorForeground": "#c3303a",
    "list.focusBackground": "#afb8c3",
    "list.highlightForeground": "#375cc6",
    "list.hoverBackground": "#e0e7ef",
    "list.inactiveSelectionBackground": "#e6ecf2",
    "list.inactiveSelectionForeground": "#151a27",
    "mergeEditor.change.word.background": "#e0e7ef",
    "mergeEditor.conflict.handled.minimapOverViewRuler": "#007010",
    "mergeEditor.conflict.unhandled.minimapOverViewRuler": "#1f66af",
    "notifications.background": "#a0c2ef",
    "notifications.foreground": "#142810",
    "notifications.border": "#a0c2ef",
    "peekView.border": "#375cc6",
    "peekViewEditor.background": "#e0e7ef",
    "peekViewEditor.matchHighlightBackground": "#dae5f0",
    "peekViewResult.background": "#cfd8e3",
    "peekViewTitle.background": "#e0e7ef",
    "scrollbarSlider.activeBackground": "#afb8c3bb",
    "scrollbarSlider.background": "#e0e7ef99",
    "scrollbarSlider.hoverBackground": "#cfd8e3cc",
    "sideBar.background": "#edf4f8",
    "sideBar.foreground": "#151a27",
    "sideBarSectionHeader.background": "#e0e7ef",
    "sideBarSectionHeader.foreground": "#151a27",
    "sideBarTitle.foreground": "#375cc6",
    "statusBar.background": "#a0c2ef",
    "statusBar.debuggingBackground": "#007010",
    "statusBar.foreground": "#142810",
    "statusBar.noFolderBackground": "#a0c2ef",
    "statusBarItem.hoverBackground": "#afb8c399",
    "tab.activeBackground": "#edf4f8",
    "tab.activeBorder": "#375cc6",
    "tab.border": "#e6ecf2",
    "tab.hoverBackground": "#e0e7ef",
    "tab.inactiveBackground": "#e6ecf2",
    "tab.inactiveForeground": "#676470",
    "titleBar.activeBackground": "#edf4f8",
    "titleBar.activeForeground": "#151a27",
    "titleBar.inactiveBackground": "#edf4f8",
    "titleBar.inactiveForeground": "#676470",
    "tree.indentGuidesStroke": "#a2a6af",
    "terminal.ansiWhite": "#151a27",
    "terminal.ansiBlack": "#e0e7ef",
    "terminal.ansiBlue": "#375cc6",
    "terminal.ansiCyan": "#1f66af",
    "terminal.ansiGreen": "#007010",
    "terminal.ansiMagenta": "#80308f",
    "terminal.ansiRed": "#c3303a",
    "terminal.ansiYellow": "#805a1f",
    "terminal.ansiBrightWhite": "#676470",
    "terminal.ansiBrightBlack": "#afb8c3",
    "terminal.ansiBrightBlue": "#003faf",
    "terminal.ansiBrightCyan": "#006f70",
    "terminal.ansiBrightGreen": "#007047",
    "terminal.ansiBrightMagenta": "#5f2fba",
    "terminal.ansiBrightRed": "#b02440",
    "terminal.ansiBrightYellow": "#78542f"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8b4052",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#006f70"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#78542f"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#003faf"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#003faf"
      }
    },
    {
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#6a4a9f"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#1f66af"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#1f66af"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#3a6f00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.class",
        "support.type.primitive",
        "storage.type.type"
      ],
      "settings": {
        "foreground": "#007010"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#444fcf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage"
      ],
      "settings": {
        "foreground": "#444fcf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#676470"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#151a27"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#151a27",
        "background": "#f2c2b5"
      }
    },
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#375cc6",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#6a4a9f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.tag",
        "entity.name.tag",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#444fcf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#007010"
      }
    },
    {
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#3a6f00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#5f2fba",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function.lisp",
      "settings": {
        "foreground": "#3a6f00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.lisp",
      "settings": {
        "foreground": "#444fcf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language.lisp",
      "settings": {
        "foreground": "#003faf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type.function.lisp",
      "settings": {
        "foreground": "#006f70",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "rx-escape",
      "settings": {
        "foreground": "#805a1f"
      }
    },
    {
      "scope": "rx-construct",
      "settings": {
        "foreground": "#c3303a"
      }
    },
    {
      "name": "Line number active",
      "scope": "editorLineNumber.activeForeground",
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    }
  ]
}
