{
  "name": "Ef-Tritanopia-Dark",
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "activityBar.background": "#15050f",
    "activityBar.foreground": "#dfd0d5",
    "activityBar.inactiveForeground": "#90889099",
    "activityBarBadge.background": "#671822",
    "activityBarBadge.foreground": "#ffffff",
    "badge.background": "#671822",
    "badge.foreground": "#ffffff",
    "breadcrumb.activeSelectionForeground": "#3fafcf",
    "breadcrumb.background": "#15050f",
    "breadcrumb.focusForeground": "#3fafcf",
    "breadcrumb.foreground": "#908890",
    "breadcrumbPicker.background": "#352f2f",
    "button.background": "#671822",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#cf4f5f33",
    "checkbox.border": "#555564",
    "debugToolBar.background": "#15050f",
    "diffEditor.insertedTextBackground": "#2fa52633",
    "dropdown.background": "#15050f",
    "dropdown.border": "#352f2f",
    "editor.background": "#15050f",
    "editor.foreground": "#dfd0d5",
    "editor.findMatchHighlightBackground": "#c4870233",
    "editor.lineHighlightBackground": "#3f1515",
    "editor.selectionBackground": "#202d3f",
    "editor.selectionForeground": "#ffffff",
    "editor.inactiveSelectionBackground": "#202d3f80",
    "editor.selectionHighlightBackground": "#202d3f40",
    "editorCursor.foreground": "#fd3333",
    "editorCursor.background": "#15050f",
    "editorError.foreground": "#df4f4f",
    "editorGroup.border": "#352f2f",
    "editorGroupHeader.tabsBackground": "#1a1517",
    "editorGroupHeader.tabsBorder": "#352f2f",
    "editorHoverWidget.background": "#352f2f",
    "editorHoverWidget.border": "#555564",
    "editorIndentGuide.background": "#282026",
    "editorLineNumber.foreground": "#908890",
    "editorLineNumber.activeForeground": "#ffffff",
    "editorLink.activeForeground": "#3fafcf",
    "editorRuler.foreground": "#555564",
    "editorSuggestWidget.background": "#352f2f",
    "editorSuggestWidget.border": "#671822",
    "editorSuggestWidget.selectedBackground": "#282026",
    "editorSuggestWidget.selectedForeground": "#dfd0d5",
    "editorWhitespace.foreground": "#555564",
    "editorWidget.background": "#352f2f",
    "editorWidget.foreground": "#ffffff",
    "extensionButton.prominentBackground": "#67182233",
    "gitDecoration.addedResourceForeground": "#cbdfff",
    "gitDecoration.modifiedResourceForeground": "#e3cfff",
    "gitDecoration.deletedResourceForeground": "#ffbfbf",
    "input.background": "#554f4f",
    "input.border": "#671822",
    "input.foreground": "#dfd0d5",
    "inputOption.activeBorder": "#3fafcf",
    "list.activeSelectionBackground": "#554f4f",
    "list.activeSelectionForeground": "#dfd0d5",
    "list.dropBackground": "#202d3f",
    "list.errorForeground": "#cf4f5f",
    "list.focusBackground": "#554f4f",
    "list.highlightForeground": "#b0648f",
    "list.hoverBackground": "#282026",
    "list.inactiveSelectionBackground": "#1a1517",
    "list.inactiveSelectionForeground": "#dfd0d5",
    "mergeEditor.change.word.background": "#282026",
    "mergeEditor.conflict.handled.minimapOverViewRuler": "#b0648f",
    "mergeEditor.conflict.unhandled.minimapOverViewRuler": "#3fafcf",
    "notifications.background": "#671822",
    "notifications.foreground": "#ffffff",
    "notifications.border": "#671822",
    "peekView.border": "#b0648f",
    "peekViewEditor.background": "#282026",
    "peekViewEditor.matchHighlightBackground": "#3f1515",
    "peekViewResult.background": "#352f2f",
    "peekViewTitle.background": "#282026",
    "scrollbarSlider.activeBackground": "#554f4fbb",
    "scrollbarSlider.background": "#28202699",
    "scrollbarSlider.hoverBackground": "#352f2fcc",
    "sideBar.background": "#15050f",
    "sideBar.foreground": "#dfd0d5",
    "sideBar.border": "#352f2f",
    "sideBarSectionHeader.background": "#282026",
    "sideBarSectionHeader.foreground": "#dfd0d5",
    "sideBarTitle.foreground": "#b0648f",
    "statusBar.background": "#671822",
    "statusBar.debuggingBackground": "#b0648f",
    "statusBar.foreground": "#ffffff",
    "statusBar.noFolderBackground": "#671822",
    "statusBarItem.hoverBackground": "#554f4f99",
    "tab.activeBackground": "#15050f",
    "tab.activeBorder": "#3fafcf",
    "tab.border": "#1a1517",
    "tab.hoverBackground": "#282026",
    "tab.inactiveBackground": "#1a1517",
    "tab.inactiveForeground": "#908890",
    "titleBar.activeBackground": "#15050f",
    "titleBar.activeForeground": "#dfd0d5",
    "titleBar.inactiveBackground": "#15050f",
    "titleBar.inactiveForeground": "#908890",
    "tree.indentGuidesStroke": "#555564",
    "terminal.ansiWhite": "#dfd0d5",
    "terminal.ansiBlack": "#282026",
    "terminal.ansiBlue": "#379cf6",
    "terminal.ansiCyan": "#3fafcf",
    "terminal.ansiGreen": "#2fa526",
    "terminal.ansiMagenta": "#b2608f",
    "terminal.ansiRed": "#cf4f5f",
    "terminal.ansiYellow": "#c48702",
    "terminal.ansiBrightWhite": "#908890",
    "terminal.ansiBrightBlack": "#554f4f",
    "terminal.ansiBrightBlue": "#029fff",
    "terminal.ansiBrightCyan": "#4fafaf",
    "terminal.ansiBrightGreen": "#00b066",
    "terminal.ansiBrightMagenta": "#a6699f",
    "terminal.ansiBrightRed": "#d24f7f",
    "terminal.ansiBrightYellow": "#df8f6f"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#b07f7f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#3fafcf"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#555564"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#d24f7f"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#d24f7f"
      }
    },
    {
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#b2608f"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#4fafaf"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#4fafaf"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#a6699f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.class",
        "support.type.primitive",
        "storage.type.type"
      ],
      "settings": {
        "foreground": "#3f9aaf"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#cf4f5f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage"
      ],
      "settings": {
        "foreground": "#cf4f5f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#908890"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#dfd0d5"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#dfd0d5",
        "background": "#3a0f0f"
      }
    },
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#3fafcf",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#c590af",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.tag",
        "entity.name.tag",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#b0648f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#3f9aaf"
      }
    },
    {
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#a6699f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#a6699f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function.lisp",
      "settings": {
        "foreground": "#a6699f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.lisp",
      "settings": {
        "foreground": "#cf4f5f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language.lisp",
      "settings": {
        "foreground": "#d24f7f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type.function.lisp",
      "settings": {
        "foreground": "#df8f6f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "rx-escape",
      "settings": {
        "foreground": "#4fafaf"
      }
    },
    {
      "scope": "rx-construct",
      "settings": {
        "foreground": "#cf4f5f"
      }
    },
    {
      "name": "Line number active",
      "scope": "editorLineNumber.activeForeground",
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    }
  ]
}
