{
  "name": "Ef-Frost",
  "$schema": "vscode://schemas/color-theme",
  "type": "light",
  "colors": {
    "activityBar.background": "#fcffff",
    "activityBar.foreground": "#232323",
    "activityBar.inactiveForeground": "#66657f99",
    "activityBarBadge.background": "#9ad0ff",
    "activityBarBadge.foreground": "#051524",
    "badge.background": "#9ad0ff",
    "badge.foreground": "#051524",
    "breadcrumb.activeSelectionForeground": "#007a85",
    "breadcrumb.background": "#fcffff",
    "breadcrumb.focusForeground": "#007a85",
    "breadcrumb.foreground": "#66657f",
    "breadcrumbPicker.background": "#d7dbdb",
    "button.background": "#9ad0ff",
    "button.foreground": "#051524",
    "button.hoverBackground": "#00845f18",
    "checkbox.border": "#b0b7c0",
    "debugToolBar.background": "#fcffff",
    "diffEditor.insertedTextBackground": "#c9ffea33",
    "dropdown.background": "#fcffff",
    "dropdown.border": "#d7dbdb",
    "editor.background": "#fcffff",
    "editor.foreground": "#232323",
    "editor.findMatchHighlightBackground": "#fac20033",
    "editor.lineHighlightBackground": "#dff6e4",
    "editor.selectionBackground": "#d4eaf3",
    "editor.selectionForeground": "#000000",
    "editor.inactiveSelectionBackground": "#d4eaf380",
    "editor.selectionHighlightBackground": "#d4eaf340",
    "editorCursor.foreground": "#0055bb",
    "editorCursor.background": "#fcffff",
    "editorError.foreground": "#c42d2f",
    "editorGroup.border": "#d7dbdb",
    "editorGroupHeader.tabsBackground": "#f7f9f9",
    "editorGroupHeader.tabsBorder": "#d7dbdb",
    "editorHoverWidget.background": "#d7dbdb",
    "editorHoverWidget.border": "#b0b7c0",
    "editorIndentGuide.background": "#eaefef",
    "editorLineNumber.foreground": "#66657f",
    "editorLineNumber.activeForeground": "#000000",
    "editorLink.activeForeground": "#1f6fbf",
    "editorRuler.foreground": "#b0b7c0",
    "editorSuggestWidget.background": "#d7dbdb",
    "editorSuggestWidget.border": "#9ad0ff",
    "editorSuggestWidget.selectedBackground": "#eaefef",
    "editorSuggestWidget.selectedForeground": "#232323",
    "editorWhitespace.foreground": "#b0b7c0",
    "editorWidget.background": "#d7dbdb",
    "extensionButton.prominentBackground": "#9ad0ff33",
    "gitDecoration.addedResourceForeground": "#004840",
    "gitDecoration.modifiedResourceForeground": "#553d00",
    "gitDecoration.untrackedResourceForeground": "#23232388",
    "input.background": "#b5b8b8",
    "input.border": "#9ad0ff",
    "input.foreground": "#232323",
    "inputOption.activeBorder": "#1f6fbf",
    "list.activeSelectionBackground": "#b5b8b8",
    "list.activeSelectionForeground": "#232323",
    "list.dropBackground": "#d4eaf3",
    "list.errorForeground": "#c42d2f",
    "list.focusBackground": "#b5b8b8",
    "list.highlightForeground": "#1f6fbf",
    "list.hoverBackground": "#eaefef",
    "list.inactiveSelectionBackground": "#f7f9f9",
    "list.inactiveSelectionForeground": "#232323",
    "mergeEditor.change.word.background": "#eaefef",
    "mergeEditor.conflict.handled.minimapOverViewRuler": "#008a00",
    "mergeEditor.conflict.unhandled.minimapOverViewRuler": "#1f6fbf",
    "notifications.background": "#9ad0ff",
    "notifications.foreground": "#051524",
    "notifications.border": "#9ad0ff",
    "peekView.border": "#1f6fbf",
    "peekViewEditor.background": "#eaefef",
    "peekViewEditor.matchHighlightBackground": "#dff6e4",
    "peekViewResult.background": "#d7dbdb",
    "peekViewTitle.background": "#eaefef",
    "scrollbarSlider.activeBackground": "#b5b8b8bb",
    "scrollbarSlider.background": "#eaefef99",
    "scrollbarSlider.hoverBackground": "#d7dbdbcc",
    "sideBar.background": "#fcffff",
    "sideBar.foreground": "#232323",
    "sideBarSectionHeader.background": "#eaefef",
    "sideBarSectionHeader.foreground": "#232323",
    "sideBarTitle.foreground": "#1f6fbf",
    "statusBar.background": "#9ad0ff",
    "statusBar.debuggingBackground": "#008a00",
    "statusBar.foreground": "#051524",
    "statusBar.noFolderBackground": "#9ad0ff",
    "statusBarItem.hoverBackground": "#b5b8b899",
    "tab.activeBackground": "#fcffff",
    "tab.activeBorder": "#1f6fbf",
    "tab.border": "#f7f9f9",
    "tab.hoverBackground": "#eaefef",
    "tab.inactiveBackground": "#f7f9f9",
    "tab.inactiveForeground": "#66657f",
    "titleBar.activeBackground": "#fcffff",
    "titleBar.activeForeground": "#232323",
    "titleBar.inactiveBackground": "#fcffff",
    "titleBar.inactiveForeground": "#66657f",
    "tree.indentGuidesStroke": "#b0b7c0",
    "terminal.ansiWhite": "#232323",
    "terminal.ansiBlack": "#eaefef",
    "terminal.ansiBlue": "#4244ef",
    "terminal.ansiCyan": "#3a6dd2",
    "terminal.ansiGreen": "#008a00",
    "terminal.ansiMagenta": "#aa44c5",
    "terminal.ansiRed": "#c42d2f",
    "terminal.ansiYellow": "#aa6100",
    "terminal.ansiBrightWhite": "#66657f",
    "terminal.ansiBrightBlack": "#b5b8b8",
    "terminal.ansiBrightBlue": "#065fff",
    "terminal.ansiBrightCyan": "#1f6fbf",
    "terminal.ansiBrightGreen": "#00845f",
    "terminal.ansiBrightMagenta": "#7f5ae0",
    "terminal.ansiBrightRed": "#cf2f4f",
    "terminal.ansiBrightYellow": "#996c4f"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7a5f2f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#4244ef"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#996c4f"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#065fff"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#065fff"
      }
    },
    {
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#065fff"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#3a6dd2"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#3a6dd2"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#00845f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.class",
        "support.type.primitive",
        "storage.type.type"
      ],
      "settings": {
        "foreground": "#7f5ae0"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#004fc0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage"
      ],
      "settings": {
        "foreground": "#004fc0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#66657f"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#232323"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#232323",
        "background": "#ffdfda"
      }
    },
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#1f6fbf",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#605f9f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.tag",
        "entity.name.tag",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#004fc0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#7f5ae0"
      }
    },
    {
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#00845f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#aa44c5",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function.lisp",
      "settings": {
        "foreground": "#00845f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.lisp",
      "settings": {
        "foreground": "#004fc0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language.lisp",
      "settings": {
        "foreground": "#065fff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type.function.lisp",
      "settings": {
        "foreground": "#00845f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "rx-escape",
      "settings": {
        "foreground": "#996c4f"
      }
    },
    {
      "scope": "rx-construct",
      "settings": {
        "foreground": "#cf2f4f"
      }
    },
    {
      "name": "Line number active",
      "scope": "editorLineNumber.activeForeground",
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    }
  ]
}
