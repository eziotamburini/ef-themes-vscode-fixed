{
  "name": "Ef-Bio",
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "activityBar.background": "#111111",
    "activityBar.foreground": "#cfdfd5",
    "activityBar.inactiveForeground": "#808f8099",
    "activityBarBadge.background": "#00552f",
    "activityBarBadge.foreground": "#d0ffe0",
    "badge.background": "#00552f",
    "badge.foreground": "#111111",
    "breadcrumb.activeSelectionForeground": "#00c089",
    "breadcrumb.background": "#111111",
    "breadcrumb.focusForeground": "#00c089",
    "breadcrumb.foreground": "#808f80",
    "breadcrumbPicker.background": "#222522",
    "button.background": "#00c08933",
    "button.foreground": "#cfdfd5",
    "button.hoverBackground": "#00c08955",
    "checkbox.border": "#505250",
    "debugToolBar.background": "#111111",
    "diffEditor.insertedTextBackground": "#00c08933",
    "dropdown.background": "#111111",
    "dropdown.border": "#303230",
    "editor.background": "#111111",
    "editor.foreground": "#cfdfd5",
    "editor.findMatchHighlightBackground": "#5a300099",
    "editor.lineHighlightBackground": "#00331f",
    "editor.selectionBackground": "#3a3027",
    "editorCursor.foreground": "#35f038",
    "editorError.foreground": "#ef6560",
    "editorGroup.border": "#303230",
    "editorGroupHeader.tabsBackground": "#161916",
    "editorGroupHeader.tabsBorder": "#303230",
    "editorHoverWidget.background": "#303230",
    "editorHoverWidget.border": "#505250",
    "editorIndentGuide.background": "#222522",
    "editorLineNumber.foreground": "#808f80",
    "editorLink.activeForeground": "#37aff6",
    "editorRuler.foreground": "#3a3a3a",
    "editorSuggestWidget.background": "#303230",
    "editorSuggestWidget.border": "#00c089",
    "editorSuggestWidget.selectedBackground": "#222522",
    "editorSuggestWidget.selectedForeground": "#cfdfd5",
    "editorWhitespace.foreground": "#3a3a3a",
    "editorWidget.background": "#303230",
    "extensionButton.prominentBackground": "#00c08933",
    "gitDecoration.addedResourceForeground": "#a0e0a0",
    "gitDecoration.modifiedResourceForeground": "#efef80",
    "gitDecoration.untrackedResourceForeground": "#cfdfd580",
    "input.background": "#505250",
    "input.border": "#00c089",
    "input.foreground": "#cfdfd5",
    "inputOption.activeBorder": "#00c089",
    "list.activeSelectionBackground": "#505250",
    "list.activeSelectionForeground": "#cfdfd5",
    "list.dropBackground": "#3a302799",
    "list.errorForeground": "#ef6560",
    "list.focusBackground": "#505250",
    "list.highlightForeground": "#00c089",
    "list.hoverBackground": "#222522",
    "list.inactiveSelectionBackground": "#161916",
    "list.inactiveSelectionForeground": "#cfdfd5",
    "mergeEditor.change.word.background": "#222522",
    "mergeEditor.conflict.handled.minimapOverViewRuler": "#00c089",
    "mergeEditor.conflict.unhandled.minimapOverViewRuler": "#7fcfdf",
    "notifications.background": "#00552f",
    "notifications.foreground": "#d0ffe0",
    "notifications.border": "#00552f",
    "peekView.border": "#00c089",
    "peekViewEditor.background": "#222522",
    "peekViewEditor.matchHighlightBackground": "#3a3027",
    "peekViewResult.background": "#222522",
    "peekViewTitle.background": "#222522",
    "scrollbarSlider.activeBackground": "#505250aa",
    "scrollbarSlider.background": "#22252288",
    "scrollbarSlider.hoverBackground": "#303230aa",
    "sideBar.background": "#111111",
    "sideBar.foreground": "#cfdfd5",
    "sideBarSectionHeader.background": "#222522",
    "sideBarSectionHeader.foreground": "#cfdfd5",
    "sideBarTitle.foreground": "#00c089",
    "statusBar.background": "#00552f",
    "statusBar.debuggingBackground": "#00c089",
    "statusBar.foreground": "#d0ffe0",
    "statusBar.noFolderBackground": "#00552f",
    "statusBarItem.hoverBackground": "#3fb83f77",
    "tab.activeBackground": "#111111",
    "tab.activeBorder": "#00c089",
    "tab.border": "#161916",
    "tab.hoverBackground": "#222522",
    "tab.inactiveBackground": "#161916",
    "tab.inactiveForeground": "#808f80",
    "titleBar.activeBackground": "#111111",
    "titleBar.activeForeground": "#cfdfd5",
    "titleBar.inactiveBackground": "#111111",
    "titleBar.inactiveForeground": "#808f80",
    "tree.indentGuidesStroke": "#3a3a3a",
    "terminal.ansiWhite": "#cfdfd5",
    "terminal.ansiBlack": "#222522",
    "terminal.ansiBlue": "#37aff6",
    "terminal.ansiCyan": "#6fc5ef",
    "terminal.ansiGreen": "#3fb83f",
    "terminal.ansiMagenta": "#d38faf",
    "terminal.ansiRed": "#ef6560",
    "terminal.ansiYellow": "#d4aa02",
    "terminal.ansiBrightWhite": "#808f80",
    "terminal.ansiBrightBlack": "#505250",
    "terminal.ansiBrightBlue": "#32cfef",
    "terminal.ansiBrightCyan": "#5dc0aa",
    "terminal.ansiBrightGreen": "#00c089",
    "terminal.ansiBrightMagenta": "#af9fff",
    "terminal.ansiBrightRed": "#ff778f",
    "terminal.ansiBrightYellow": "#cfc04f"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#b7a07f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#af9fff"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#525959"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#cfdfd5"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#37aff6"
      }
    },
    {
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#3fb83f"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#78afff"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#78afff"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#7fc500"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.class",
        "support.type.primitive",
        "storage.type.type"
      ],
      "settings": {
        "foreground": "#00c089"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#00c089"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage"
      ],
      "settings": {
        "foreground": "#00c089"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#808f80"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#cfdfd5"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#cfdfd5",
        "background": "#381400"
      }
    },
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#37aff6",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#37aff6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.tag",
        "entity.name.tag",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#00c089"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#7fcfdf"
      }
    }
  ]
}

