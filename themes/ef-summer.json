{
  "name": "Ef-Summer",
  "$schema": "vscode://schemas/color-theme",
  "type": "light",
  "colors": {
    "activityBar.background": "#fff2f3",
    "activityBar.foreground": "#4f4073",
    "activityBar.inactiveForeground": "#786e7499",
    "activityBarBadge.background": "#ffa4dc",
    "activityBarBadge.foreground": "#341f58",
    "badge.background": "#ffa4dc",
    "badge.foreground": "#341f58",
    "breadcrumb.activeSelectionForeground": "#ba35af",
    "breadcrumb.background": "#fff2f3",
    "breadcrumb.focusForeground": "#ba35af",
    "breadcrumb.foreground": "#786e74",
    "breadcrumbPicker.background": "#f2e4ea",
    "button.background": "#ffa4dc",
    "button.foreground": "#341f58",
    "button.hoverBackground": "#ba35af33",
    "checkbox.border": "#c6bbc6",
    "debugToolBar.background": "#fff2f3",
    "diffEditor.insertedTextBackground": "#217a3c33",
    "dropdown.background": "#fff2f3",
    "dropdown.border": "#f2e4ea",
    "editor.background": "#fff2f3",
    "editor.foreground": "#4f4073",
    "editor.findMatchHighlightBackground": "#ffc20033",
    "editor.lineHighlightBackground": "#ffd6e5",
    "editor.selectionBackground": "#eecfff",
    "editor.selectionForeground": "#000000",
    "editor.inactiveSelectionBackground": "#eecfff80",
    "editor.selectionHighlightBackground": "#eecfff40",
    "editorCursor.foreground": "#cf0090",
    "editorCursor.background": "#fff2f3",
    "editorError.foreground": "#e00033",
    "editorGroup.border": "#f2e4ea",
    "editorGroupHeader.tabsBackground": "#f7ebee",
    "editorGroupHeader.tabsBorder": "#f2e4ea",
    "editorHoverWidget.background": "#f2e4ea",
    "editorHoverWidget.border": "#c6bbc6",
    "editorIndentGuide.background": "#efd3e4",
    "editorLineNumber.foreground": "#786e74",
    "editorLineNumber.activeForeground": "#000000",
    "editorLink.activeForeground": "#375ce6",
    "editorRuler.foreground": "#c6bbc6",
    "editorSuggestWidget.background": "#f2e4ea",
    "editorSuggestWidget.border": "#ffa4dc",
    "editorSuggestWidget.selectedBackground": "#efd3e4",
    "editorSuggestWidget.selectedForeground": "#4f4073",
    "editorWhitespace.foreground": "#c6bbc6",
    "editorWidget.background": "#f2e4ea",
    "extensionButton.prominentBackground": "#ffa4dc33",
    "gitDecoration.addedResourceForeground": "#005000",
    "gitDecoration.modifiedResourceForeground": "#553d00",
    "gitDecoration.untrackedResourceForeground": "#4f407388",
    "input.background": "#cfb3c4",
    "input.border": "#ffa4dc",
    "input.foreground": "#4f4073",
    "inputOption.activeBorder": "#375ce6",
    "list.activeSelectionBackground": "#cfb3c4",
    "list.activeSelectionForeground": "#4f4073",
    "list.dropBackground": "#eecfff",
    "list.errorForeground": "#d3303a",
    "list.focusBackground": "#cfb3c4",
    "list.highlightForeground": "#ba35af",
    "list.hoverBackground": "#f2e4ea",
    "list.inactiveSelectionBackground": "#f7ebee",
    "list.inactiveSelectionForeground": "#4f4073",
    "mergeEditor.change.word.background": "#f2e4ea",
    "mergeEditor.conflict.handled.minimapOverViewRuler": "#a45f22",
    "mergeEditor.conflict.unhandled.minimapOverViewRuler": "#007f68",
    "notifications.background": "#ffa4dc",
    "notifications.foreground": "#341f58",
    "notifications.border": "#ffa4dc",
    "peekView.border": "#a45f22",
    "peekViewEditor.background": "#f2e4ea",
    "peekViewEditor.matchHighlightBackground": "#ffd6e5",
    "peekViewResult.background": "#f2e4ea",
    "peekViewTitle.background": "#f2e4ea",
    "scrollbarSlider.activeBackground": "#cfb3c4bb",
    "scrollbarSlider.background": "#f2e4ea99",
    "scrollbarSlider.hoverBackground": "#efd3e4cc",
    "sideBar.background": "#fff2f3",
    "sideBar.foreground": "#4f4073",
    "sideBarSectionHeader.background": "#f2e4ea",
    "sideBarSectionHeader.foreground": "#4f4073",
    "sideBarTitle.foreground": "#ba35af",
    "statusBar.background": "#ffa4dc",
    "statusBar.debuggingBackground": "#a45f22",
    "statusBar.foreground": "#341f58",
    "statusBar.noFolderBackground": "#ffa4dc",
    "statusBarItem.hoverBackground": "#ba35af33",
    "tab.activeBackground": "#fff2f3",
    "tab.activeBorder": "#375ce6",
    "tab.border": "#f7ebee",
    "tab.hoverBackground": "#f2e4ea",
    "tab.inactiveBackground": "#f7ebee",
    "tab.inactiveForeground": "#786e74",
    "titleBar.activeBackground": "#fff2f3",
    "titleBar.activeForeground": "#4f4073",
    "titleBar.inactiveBackground": "#fff2f3",
    "titleBar.inactiveForeground": "#786e74",
    "tree.indentGuidesStroke": "#c6bbc6",
    "terminal.ansiWhite": "#4f4073",
    "terminal.ansiBlack": "#f2e4ea",
    "terminal.ansiBlue": "#375ce6",
    "terminal.ansiCyan": "#1f6fbf",
    "terminal.ansiGreen": "#217a3c",
    "terminal.ansiMagenta": "#ba35af",
    "terminal.ansiRed": "#d3303a",
    "terminal.ansiYellow": "#a45f22",
    "terminal.ansiBrightWhite": "#786e74",
    "terminal.ansiBrightBlack": "#cfb3c4",
    "terminal.ansiBrightBlue": "#065fff",
    "terminal.ansiBrightCyan": "#0f7b8f",
    "terminal.ansiBrightGreen": "#007f68",
    "terminal.ansiBrightMagenta": "#8e44f3",
    "terminal.ansiBrightRed": "#d50f7f",
    "terminal.ansiBrightYellow": "#b65050"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#9a5f6a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#b6532f"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#c6bbc6"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#d50f7f"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#d50f7f"
      }
    },
    {
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#ba35af"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#5250ef"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#5250ef"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#cb1aaa",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.class",
        "support.type.primitive",
        "storage.type.type"
      ],
      "settings": {
        "foreground": "#3f6faf"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#8e44f3",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage"
      ],
      "settings": {
        "foreground": "#8e44f3",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#786e74"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#4f4073"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#4f4073",
        "background": "#ffd0e6"
      }
    },
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#375ce6",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#a45392",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.tag",
        "entity.name.tag",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#ba35af",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#3f6faf"
      }
    },
    {
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#cb1aaa",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#007f68",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function.lisp",
      "settings": {
        "foreground": "#cb1aaa",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.lisp",
      "settings": {
        "foreground": "#8e44f3",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language.lisp",
      "settings": {
        "foreground": "#d50f7f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type.function.lisp",
      "settings": {
        "foreground": "#a45f22",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "rx-escape",
      "settings": {
        "foreground": "#0f7b8f"
      }
    },
    {
      "scope": "rx-construct",
      "settings": {
        "foreground": "#d50f7f"
      }
    },
    {
      "name": "Line number active",
      "scope": "editorLineNumber.activeForeground",
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    }
  ]
}
