{
  "name": "Ef-Deuteranopia-Light",
  "$schema": "vscode://schemas/color-theme",
  "type": "light",
  "colors": {
    "activityBar.background": "#f5f5ff",
    "activityBar.foreground": "#1a1a2f",
    "activityBar.inactiveForeground": "#70627f99",
    "activityBarBadge.background": "#99c7ff",
    "activityBarBadge.foreground": "#0a0a1f",
    "badge.background": "#99c7ff",
    "badge.foreground": "#0a0a1f",
    "breadcrumb.activeSelectionForeground": "#4250ef",
    "breadcrumb.background": "#f5f5ff",
    "breadcrumb.focusForeground": "#4250ef",
    "breadcrumb.foreground": "#70627f",
    "breadcrumbPicker.background": "#d3d3e0",
    "button.background": "#99c7ff",
    "button.foreground": "#0a0a1f",
    "button.hoverBackground": "#3f90f018",
    "checkbox.border": "#bcbcd0",
    "debugToolBar.background": "#f5f5ff",
    "diffEditor.insertedTextBackground": "#dbdbff33",
    "dropdown.background": "#f5f5ff",
    "dropdown.border": "#d3d3e0",
    "editor.background": "#f5f5ff",
    "editor.foreground": "#1a1a2f",
    "editor.findMatchHighlightBackground": "#fac20033",
    "editor.lineHighlightBackground": "#f3e0d5",
    "editor.selectionBackground": "#dadadf",
    "editor.selectionForeground": "#000000",
    "editor.inactiveSelectionBackground": "#dadadf80",
    "editor.selectionHighlightBackground": "#dadadf40",
    "editorCursor.foreground": "#0000bb",
    "editorCursor.background": "#f5f5ff",
    "editorError.foreground": "#965000",
    "editorGroup.border": "#d3d3e0",
    "editorGroupHeader.tabsBackground": "#efeff5",
    "editorGroupHeader.tabsBorder": "#d3d3e0",
    "editorHoverWidget.background": "#d3d3e0",
    "editorHoverWidget.border": "#bcbcd0",
    "editorIndentGuide.background": "#e8e8ea",
    "editorLineNumber.foreground": "#70627f",
    "editorLineNumber.activeForeground": "#000000",
    "editorLink.activeForeground": "#375cd8",
    "editorRuler.foreground": "#bcbcd0",
    "editorSuggestWidget.background": "#d3d3e0",
    "editorSuggestWidget.border": "#99c7ff",
    "editorSuggestWidget.selectedBackground": "#e8e8ea",
    "editorSuggestWidget.selectedForeground": "#1a1a2f",
    "editorWhitespace.foreground": "#bcbcd0",
    "editorWidget.background": "#d3d3e0",
    "extensionButton.prominentBackground": "#99c7ff33",
    "gitDecoration.addedResourceForeground": "#333399",
    "gitDecoration.modifiedResourceForeground": "#6f1343",
    "gitDecoration.untrackedResourceForeground": "#1a1a2f80",
    "input.background": "#b3b3c0",
    "input.border": "#99c7ff",
    "input.foreground": "#1a1a2f",
    "inputOption.activeBorder": "#375cd8",
    "list.activeSelectionBackground": "#b3b3c0",
    "list.activeSelectionForeground": "#1a1a2f",
    "list.dropBackground": "#dadadf99",
    "list.errorForeground": "#d3303a",
    "list.focusBackground": "#b3b3c0",
    "list.highlightForeground": "#4250ef",
    "list.hoverBackground": "#e8e8ea",
    "list.inactiveSelectionBackground": "#efeff5",
    "list.inactiveSelectionForeground": "#1a1a2f",
    "mergeEditor.change.word.background": "#e8e8ea",
    "mergeEditor.conflict.handled.minimapOverViewRuler": "#4250ef",
    "mergeEditor.conflict.unhandled.minimapOverViewRuler": "#1f77bb",
    "notifications.background": "#99c7ff",
    "notifications.foreground": "#0a0a1f",
    "notifications.border": "#99c7ff",
    "peekView.border": "#4250ef",
    "peekViewEditor.background": "#e8e8ea",
    "peekViewEditor.matchHighlightBackground": "#f0e0a0",
    "peekViewResult.background": "#d3d3e0",
    "peekViewTitle.background": "#e8e8ea",
    "scrollbarSlider.activeBackground": "#d3d3e0",
    "scrollbarSlider.background": "#e8e8ea",
    "scrollbarSlider.hoverBackground": "#e8e8ea",
    "sideBar.background": "#f5f5ff",
    "sideBar.foreground": "#1a1a2f",
    "sideBarSectionHeader.background": "#e8e8ea",
    "sideBarSectionHeader.foreground": "#1a1a2f",
    "sideBarTitle.foreground": "#4250ef",
    "statusBar.background": "#99c7ff",
    "statusBar.debuggingBackground": "#4250ef",
    "statusBar.foreground": "#0a0a1f",
    "statusBar.noFolderBackground": "#99c7ff",
    "statusBarItem.hoverBackground": "#b3b3c077",
    "tab.activeBackground": "#f5f5ff",
    "tab.activeBorder": "#375cd8",
    "tab.border": "#efeff5",
    "tab.hoverBackground": "#e8e8ea",
    "tab.inactiveBackground": "#efeff5",
    "tab.inactiveForeground": "#70627f",
    "titleBar.activeBackground": "#f5f5ff",
    "titleBar.activeForeground": "#1a1a2f",
    "titleBar.inactiveBackground": "#f5f5ff",
    "titleBar.inactiveForeground": "#70627f",
    "tree.indentGuidesStroke": "#bcbcd0",
    "terminal.ansiWhite": "#1a1a2f",
    "terminal.ansiBlack": "#e8e8ea",
    "terminal.ansiBlue": "#375cd8",
    "terminal.ansiCyan": "#1f6fbf",
    "terminal.ansiGreen": "#217a3c",
    "terminal.ansiMagenta": "#ba35af",
    "terminal.ansiRed": "#d3303a",
    "terminal.ansiYellow": "#805d00",
    "terminal.ansiBrightWhite": "#70627f",
    "terminal.ansiBrightBlack": "#b3b3c0",
    "terminal.ansiBrightBlue": "#065fff",
    "terminal.ansiBrightCyan": "#1f77bb",
    "terminal.ansiBrightGreen": "#008058",
    "terminal.ansiBrightMagenta": "#6052cf",
    "terminal.ansiBrightRed": "#d50f7f",
    "terminal.ansiBrightYellow": "#765040"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#77604a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#965000"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#bcbcd0"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#765040"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#765040"
      }
    },
    {
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#1f6fbf"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#1f77bb"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#1f77bb"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#065fff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.class",
        "support.type.primitive",
        "storage.type.type"
      ],
      "settings": {
        "foreground": "#805d00"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#4250ef",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage"
      ],
      "settings": {
        "foreground": "#4250ef",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#70627f"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#1a1a2f"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#1a1a2f",
        "background": "#f0e0a0"
      }
    },
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#375cd8",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#506fa0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.tag",
        "entity.name.tag",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#4250ef",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#805d00"
      }
    },
    {
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#065fff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#6052cf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function.lisp",
      "settings": {
        "foreground": "#065fff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.lisp",
      "settings": {
        "foreground": "#4250ef",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language.lisp",
      "settings": {
        "foreground": "#1f6fbf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type.function.lisp",
      "settings": {
        "foreground": "#217a3c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "rx-escape",
      "settings": {
        "foreground": "#506fa0"
      }
    },
    {
      "scope": "rx-construct",
      "settings": {
        "foreground": "#4250ef"
      }
    },
    {
      "name": "Line number active",
      "scope": "editorLineNumber.activeForeground",
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    }
  ]
}
