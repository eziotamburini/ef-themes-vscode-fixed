{
  "name": "Ef-Dark",
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "activityBar.background": "#000000",
    "activityBar.foreground": "#d0d0d0",
    "activityBar.inactiveForeground": "#857f8f99",
    "activityBarBadge.background": "#2a2a75",
    "activityBarBadge.foreground": "#e0e0ff",
    "badge.background": "#2a2a75",
    "badge.foreground": "#e0e0ff",
    "breadcrumb.activeSelectionForeground": "#af85ff",
    "breadcrumb.background": "#000000",
    "breadcrumb.focusForeground": "#af85ff",
    "breadcrumb.foreground": "#857f8f",
    "breadcrumbPicker.background": "#2b2b2b",
    "button.background": "#2a2a7518",
    "button.foreground": "#d0d0d0",
    "button.hoverBackground": "#2a2a7533",
    "checkbox.border": "#4b4b4b",
    "debugToolBar.background": "#000000",
    "diffEditor.insertedTextBackground": "#00a69233",
    "dropdown.background": "#000000",
    "dropdown.border": "#2b2b2b",
    "editor.background": "#000000",
    "editor.foreground": "#d0d0d0",
    "editor.findMatchHighlightBackground": "#60300099",
    "editor.lineHighlightBackground": "#002435",
    "editor.selectionBackground": "#2a234a",
    "editor.selectionForeground": "#ffffff",
    "editor.inactiveSelectionBackground": "#2a234a80",
    "editor.selectionHighlightBackground": "#2a234a40",
    "editorCursor.foreground": "#ff76ff",
    "editorCursor.background": "#000000",
    "editorError.foreground": "#ef6560",
    "editorGroup.border": "#2b2b2b",
    "editorGroupHeader.tabsBackground": "#121212",
    "editorGroupHeader.tabsBorder": "#2b2b2b",
    "editorHoverWidget.background": "#2b2b2b",
    "editorHoverWidget.border": "#4b4b4b",
    "editorIndentGuide.background": "#1a1a1a",
    "editorLineNumber.foreground": "#857f8f",
    "editorLineNumber.activeForeground": "#ffffff",
    "editorLink.activeForeground": "#1dbfcf",
    "editorRuler.foreground": "#3a3a3a",
    "editorSuggestWidget.background": "#2b2b2b",
    "editorSuggestWidget.border": "#2a2a75",
    "editorSuggestWidget.selectedBackground": "#1a1a1a",
    "editorSuggestWidget.selectedForeground": "#d0d0d0",
    "editorWhitespace.foreground": "#3a3a3a",
    "editorWidget.background": "#2b2b2b",
    "extensionButton.prominentBackground": "#2a2a7533",
    "gitDecoration.addedResourceForeground": "#a0e0a0",
    "gitDecoration.modifiedResourceForeground": "#efef80",
    "gitDecoration.untrackedResourceForeground": "#d0d0d088",
    "input.background": "#4b4b4b",
    "input.border": "#af85ff",
    "input.foreground": "#d0d0d0",
    "inputOption.activeBorder": "#af85ff",
    "list.activeSelectionBackground": "#4b4b4b",
    "list.activeSelectionForeground": "#d0d0d0",
    "list.dropBackground": "#2a234a99",
    "list.errorForeground": "#ef6560",
    "list.focusBackground": "#4b4b4b",
    "list.highlightForeground": "#af85ff",
    "list.hoverBackground": "#1a1a1a",
    "list.inactiveSelectionBackground": "#121212",
    "list.inactiveSelectionForeground": "#d0d0d0",
    "mergeEditor.change.word.background": "#1a1a1a",
    "mergeEditor.conflict.handled.minimapOverViewRuler": "#af85ff",
    "mergeEditor.conflict.unhandled.minimapOverViewRuler": "#1dbfcf",
    "notifications.background": "#2a2a75",
    "notifications.foreground": "#e0e0ff",
    "notifications.border": "#2a2a75",
    "peekView.border": "#af85ff",
    "peekViewEditor.background": "#1a1a1a",
    "peekViewEditor.matchHighlightBackground": "#360d09",
    "peekViewResult.background": "#2a2a2a",
    "peekViewTitle.background": "#2a2a2a",
    "scrollbarSlider.activeBackground": "#4b4b4bbb",
    "scrollbarSlider.background": "#1a1a1a88",
    "scrollbarSlider.hoverBackground": "#2b2b2baa",
    "sideBar.background": "#000000",
    "sideBar.foreground": "#d0d0d0",
    "sideBarSectionHeader.background": "#1a1a1a",
    "sideBarSectionHeader.foreground": "#d0d0d0",
    "sideBarTitle.foreground": "#af85ff",
    "statusBar.background": "#2a2a75",
    "statusBar.debuggingBackground": "#af85ff",
    "statusBar.foreground": "#e0e0ff",
    "statusBar.noFolderBackground": "#2a2a75",
    "statusBarItem.hoverBackground": "#7a6add77",
    "tab.activeBackground": "#000000",
    "tab.activeBorder": "#af85ff",
    "tab.border": "#121212",
    "tab.hoverBackground": "#1a1a1a",
    "tab.inactiveBackground": "#121212",
    "tab.inactiveForeground": "#857f8f",
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#d0d0d0",
    "titleBar.inactiveBackground": "#000000",
    "titleBar.inactiveForeground": "#857f8f",
    "tree.indentGuidesStroke": "#3a3a3a",
    "terminal.ansiWhite": "#d0d0d0",
    "terminal.ansiBlack": "#1a1a1a",
    "terminal.ansiBlue": "#3f95f6",
    "terminal.ansiCyan": "#4fbaef",
    "terminal.ansiGreen": "#0faa26",
    "terminal.ansiMagenta": "#d369af",
    "terminal.ansiRed": "#ef6560",
    "terminal.ansiYellow": "#bf9032",
    "terminal.ansiBrightWhite": "#857f8f",
    "terminal.ansiBrightBlack": "#4b4b4b",
    "terminal.ansiBrightBlue": "#029fff",
    "terminal.ansiBrightCyan": "#1dbfcf",
    "terminal.ansiBrightGreen": "#00a692",
    "terminal.ansiBrightMagenta": "#af85ff",
    "terminal.ansiBrightRed": "#ff5a7a",
    "terminal.ansiBrightYellow": "#df8a5a"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#cf9f8f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#6a9fff"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#4f4f5f"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#029fff"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#029fff"
      }
    },
    {
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#029fff"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#1dbfcf"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#1dbfcf"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#e580ea",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.class",
        "support.type.primitive",
        "storage.type.type"
      ],
      "settings": {
        "foreground": "#00a692"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#af85ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage"
      ],
      "settings": {
        "foreground": "#af85ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#857f8f"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#d0d0d0"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#d0d0d0",
        "background": "#461210"
      }
    },
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#4fbaef",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#c58faf",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.tag",
        "entity.name.tag",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#d369af",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#00a692"
      }
    },
    {
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#e580ea",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#ef6560",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function.lisp",
      "settings": {
        "foreground": "#e580ea",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.lisp",
      "settings": {
        "foreground": "#af85ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language.lisp",
      "settings": {
        "foreground": "#029fff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type.function.lisp",
      "settings": {
        "foreground": "#0faa26",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "rx-escape",
      "settings": {
        "foreground": "#6aad0f"
      }
    },
    {
      "scope": "rx-construct",
      "settings": {
        "foreground": "#ef6560"
      }
    },
    {
      "name": "Line number active",
      "scope": "editorLineNumber.activeForeground",
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    }
  ]
}
