{
  "name": "Ef-Duo-Light",
  "$schema": "vscode://schemas/color-theme",
  "type": "light",
  "colors": {
    "activityBar.background": "#fff8f0",
    "activityBar.foreground": "#222222",
    "activityBar.inactiveForeground": "#63728f99",
    "activityBarBadge.background": "#f8cf8f",
    "activityBarBadge.foreground": "#111133",
    "badge.background": "#f8cf8f",
    "badge.foreground": "#111133",
    "breadcrumb.activeSelectionForeground": "#4250ef",
    "breadcrumb.background": "#fff8f0",
    "breadcrumb.focusForeground": "#4250ef",
    "breadcrumb.foreground": "#63728f",
    "breadcrumbPicker.background": "#e7e0da",
    "button.background": "#f8cf8f",
    "button.foreground": "#111133",
    "button.hoverBackground": "#f8cf8fcc",
    "checkbox.border": "#baafba",
    "debugToolBar.background": "#fff8f0",
    "diffEditor.insertedTextBackground": "#ccefcf33",
    "dropdown.background": "#fff8f0",
    "dropdown.border": "#e7e0da",
    "editor.background": "#fff8f0",
    "editor.foreground": "#222222",
    "editor.findMatchHighlightBackground": "#fac20033",
    "editor.lineHighlightBackground": "#f9e8c0",
    "editor.selectionBackground": "#caeafa",
    "editor.selectionForeground": "#000000",
    "editor.inactiveSelectionBackground": "#caeafa80",
    "editor.selectionHighlightBackground": "#caeafa40",
    "editorCursor.foreground": "#1144ff",
    "editorCursor.background": "#fff8f0",
    "editorError.foreground": "#cc3333",
    "editorGroup.border": "#e7e0da",
    "editorGroupHeader.tabsBackground": "#f9f2ef",
    "editorGroupHeader.tabsBorder": "#e7e0da",
    "editorHoverWidget.background": "#e7e0da",
    "editorHoverWidget.border": "#baafba",
    "editorIndentGuide.background": "#f6ece8",
    "editorLineNumber.foreground": "#63728f",
    "editorLineNumber.activeForeground": "#000000",
    "editorLink.activeForeground": "#1f6fbf",
    "editorRuler.foreground": "#baafba",
    "editorSuggestWidget.background": "#e7e0da",
    "editorSuggestWidget.border": "#f8cf8f",
    "editorSuggestWidget.selectedBackground": "#f6ece8",
    "editorSuggestWidget.selectedForeground": "#222222",
    "editorWhitespace.foreground": "#baafba",
    "editorWidget.background": "#e7e0da",
    "extensionButton.prominentBackground": "#f8cf8f33",
    "gitDecoration.addedResourceForeground": "#005000",
    "gitDecoration.modifiedResourceForeground": "#553d00",
    "gitDecoration.untrackedResourceForeground": "#22222288",
    "input.background": "#c7c0ba",
    "input.border": "#4250ef",
    "input.foreground": "#222222",
    "inputOption.activeBorder": "#4250ef",
    "list.activeSelectionBackground": "#c7c0ba",
    "list.activeSelectionForeground": "#222222",
    "list.dropBackground": "#caeafa99",
    "list.errorForeground": "#cc3333",
    "list.focusBackground": "#c7c0ba",
    "list.highlightForeground": "#4250ef",
    "list.hoverBackground": "#f6ece8",
    "list.inactiveSelectionBackground": "#f9f2ef",
    "list.inactiveSelectionForeground": "#222222",
    "mergeEditor.change.word.background": "#f6ece8",
    "mergeEditor.conflict.handled.minimapOverViewRuler": "#4250ef",
    "mergeEditor.conflict.unhandled.minimapOverViewRuler": "#1f77bb",
    "notifications.background": "#f8cf8f",
    "notifications.foreground": "#111133",
    "notifications.border": "#f8cf8f",
    "peekView.border": "#4250ef",
    "peekViewEditor.background": "#f6ece8",
    "peekViewEditor.matchHighlightBackground": "#fac200",
    "peekViewResult.background": "#e7e0da",
    "peekViewTitle.background": "#f6ece8",
    "scrollbarSlider.activeBackground": "#c7c0baaa",
    "scrollbarSlider.background": "#f6ece888",
    "scrollbarSlider.hoverBackground": "#e7e0daaa",
    "sideBar.background": "#fff8f0",
    "sideBar.foreground": "#222222",
    "sideBarSectionHeader.background": "#f6ece8",
    "sideBarSectionHeader.foreground": "#222222",
    "sideBarTitle.foreground": "#4250ef",
    "statusBar.background": "#f8cf8f",
    "statusBar.debuggingBackground": "#4250ef",
    "statusBar.foreground": "#111133",
    "statusBar.noFolderBackground": "#f8cf8f",
    "statusBarItem.hoverBackground": "#c7c0ba77",
    "tab.activeBackground": "#fff8f0",
    "tab.activeBorder": "#4250ef",
    "tab.border": "#f9f2ef",
    "tab.hoverBackground": "#f6ece8",
    "tab.inactiveBackground": "#f9f2ef",
    "tab.inactiveForeground": "#63728f",
    "titleBar.activeBackground": "#fff8f0",
    "titleBar.activeForeground": "#222222",
    "titleBar.inactiveBackground": "#fff8f0",
    "titleBar.inactiveForeground": "#63728f",
    "tree.indentGuidesStroke": "#baafba",
    "terminal.ansiWhite": "#222222",
    "terminal.ansiBlack": "#f6ece8",
    "terminal.ansiBlue": "#375cd8",
    "terminal.ansiCyan": "#1f6fbf",
    "terminal.ansiGreen": "#217a3c",
    "terminal.ansiMagenta": "#ba35af",
    "terminal.ansiRed": "#cc3333",
    "terminal.ansiYellow": "#8a5d00",
    "terminal.ansiBrightWhite": "#63728f",
    "terminal.ansiBrightBlack": "#c7c0ba",
    "terminal.ansiBrightBlue": "#065fff",
    "terminal.ansiBrightCyan": "#1f77bb",
    "terminal.ansiBrightGreen": "#008058",
    "terminal.ansiBrightMagenta": "#6052cf",
    "terminal.ansiBrightRed": "#c04440",
    "terminal.ansiBrightYellow": "#8f5a3a"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#a2403f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#9f4a00"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#baafba"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#375cd8"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#375cd8"
      }
    },
    {
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#1f77bb"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#6052cf"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#6052cf"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#1f6fbf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.class",
        "support.type.primitive",
        "storage.type.type"
      ],
      "settings": {
        "foreground": "#065fff"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#4250ef",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage"
      ],
      "settings": {
        "foreground": "#4250ef",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#63728f"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#222222"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#222222",
        "background": "#ffdfe6"
      }
    },
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#1f6fbf",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#3f70a0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.tag",
        "entity.name.tag",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#4250ef",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#065fff"
      }
    },
    {
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#1f6fbf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#3f70a0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function.lisp",
      "settings": {
        "foreground": "#1f6fbf",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.lisp",
      "settings": {
        "foreground": "#4250ef",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language.lisp",
      "settings": {
        "foreground": "#375cd8",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type.function.lisp",
      "settings": {
        "foreground": "#217a3c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "rx-escape",
      "settings": {
        "foreground": "#1f6fbf"
      }
    },
    {
      "scope": "rx-construct",
      "settings": {
        "foreground": "#dd1100"
      }
    },
    {
      "name": "Line number active",
      "scope": "editorLineNumber.activeForeground",
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    }
  ]
}
