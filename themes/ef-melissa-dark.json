{
  "name": "Ef-Melissa-Dark",
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "activityBar.background": "#352718",
    "activityBar.foreground": "#e8e4b1",
    "activityBar.inactiveForeground": "#90918a99",
    "activityBarBadge.background": "#704f00",
    "activityBarBadge.foreground": "#f8efd8",
    "badge.background": "#704f00",
    "badge.foreground": "#f8efd8",
    "breadcrumb.activeSelectionForeground": "#e7a06f",
    "breadcrumb.background": "#352718",
    "breadcrumb.focusForeground": "#e7a06f",
    "breadcrumb.foreground": "#90918a",
    "breadcrumbPicker.background": "#483426",
    "button.background": "#483426",
    "button.foreground": "#e8e4b1",
    "button.hoverBackground": "#65d59018",
    "checkbox.border": "#6f5f58",
    "debugToolBar.background": "#352718",
    "diffEditor.insertedTextBackground": "#2f491f33",
    "dropdown.background": "#352718",
    "dropdown.border": "#483426",
    "editor.background": "#352718",
    "editor.foreground": "#e8e4b1",
    "editor.findMatchHighlightBackground": "#e4b53f33",
    "editor.lineHighlightBackground": "#4f311f",
    "editor.selectionBackground": "#443a4f",
    "editor.selectionForeground": "#ffffff",
    "editor.inactiveSelectionBackground": "#443a4f80",
    "editor.selectionHighlightBackground": "#443a4f40",
    "editorCursor.foreground": "#f9cf7a",
    "editorCursor.background": "#352718",
    "editorError.foreground": "#ff7f4f",
    "editorGroup.border": "#483426",
    "editorGroupHeader.tabsBackground": "#3d2e1f",
    "editorGroupHeader.tabsBorder": "#483426",
    "editorHoverWidget.background": "#483426",
    "editorHoverWidget.border": "#6f5f58",
    "editorIndentGuide.background": "#59463f",
    "editorLineNumber.foreground": "#90918a",
    "editorLineNumber.activeForeground": "#ffffff",
    "editorLink.activeForeground": "#7fc5df",
    "editorRuler.foreground": "#6f5f58",
    "editorSuggestWidget.background": "#483426",
    "editorSuggestWidget.border": "#704f00",
    "editorSuggestWidget.selectedBackground": "#59463f",
    "editorSuggestWidget.selectedForeground": "#e8e4b1",
    "editorWhitespace.foreground": "#6f5f58",
    "editorWidget.background": "#483426",
    "extensionButton.prominentBackground": "#704f0033",
    "gitDecoration.addedResourceForeground": "#6fd560",
    "gitDecoration.modifiedResourceForeground": "#dada90",
    "gitDecoration.untrackedResourceForeground": "#e8e4b188",
    "input.background": "#6f5f58",
    "input.border": "#704f00",
    "input.foreground": "#e8e4b1",
    "inputOption.activeBorder": "#7fc5df",
    "list.activeSelectionBackground": "#6f5f58",
    "list.activeSelectionForeground": "#e8e4b1",
    "list.dropBackground": "#60518f",
    "list.errorForeground": "#ff7f7f",
    "list.focusBackground": "#6f5f58",
    "list.highlightForeground": "#a0d13a",
    "list.hoverBackground": "#59463f",
    "list.inactiveSelectionBackground": "#3d2e1f",
    "list.inactiveSelectionForeground": "#e8e4b1",
    "mergeEditor.change.word.background": "#483426",
    "mergeEditor.conflict.handled.minimapOverViewRuler": "#a0d13a",
    "mergeEditor.conflict.unhandled.minimapOverViewRuler": "#7fc5df",
    "notifications.background": "#704f00",
    "notifications.foreground": "#f8efd8",
    "notifications.border": "#704f00",
    "peekView.border": "#a0d13a",
    "peekViewEditor.background": "#59463f",
    "peekViewEditor.matchHighlightBackground": "#4f311f",
    "peekViewResult.background": "#483426",
    "peekViewTitle.background": "#59463f",
    "scrollbarSlider.activeBackground": "#6f5f58bb",
    "scrollbarSlider.background": "#48342699",
    "scrollbarSlider.hoverBackground": "#6f5f58cc",
    "sideBar.background": "#352718",
    "sideBar.foreground": "#e8e4b1",
    "sideBarSectionHeader.background": "#3d2e1f",
    "sideBarSectionHeader.foreground": "#e8e4b1",
    "sideBarTitle.foreground": "#a0d13a",
    "statusBar.background": "#704f00",
    "statusBar.debuggingBackground": "#a0d13a",
    "statusBar.foreground": "#f8efd8",
    "statusBar.noFolderBackground": "#704f00",
    "statusBarItem.hoverBackground": "#6f5f5899",
    "tab.activeBackground": "#352718",
    "tab.activeBorder": "#7fc5df",
    "tab.border": "#3d2e1f",
    "tab.hoverBackground": "#59463f",
    "tab.inactiveBackground": "#3d2e1f",
    "tab.inactiveForeground": "#90918a",
    "titleBar.activeBackground": "#352718",
    "titleBar.activeForeground": "#e8e4b1",
    "titleBar.inactiveBackground": "#352718",
    "titleBar.inactiveForeground": "#90918a",
    "tree.indentGuidesStroke": "#6f5f58",
    "terminal.ansiWhite": "#e8e4b1",
    "terminal.ansiBlack": "#483426",
    "terminal.ansiBlue": "#57aff6",
    "terminal.ansiCyan": "#6fcad0",
    "terminal.ansiGreen": "#6fd560",
    "terminal.ansiMagenta": "#f0aac5",
    "terminal.ansiRed": "#ff7f7f",
    "terminal.ansiYellow": "#e4b53f",
    "terminal.ansiBrightWhite": "#90918a",
    "terminal.ansiBrightBlack": "#6f5f58",
    "terminal.ansiBrightBlue": "#98bfff",
    "terminal.ansiBrightCyan": "#70e0cf",
    "terminal.ansiBrightGreen": "#65d590",
    "terminal.ansiBrightMagenta": "#c6a2fe",
    "terminal.ansiBrightRed": "#ff8f98",
    "terminal.ansiBrightYellow": "#e7a06f"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#eab780",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#ffa21f"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#6fd560"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#98bfff"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#98bfff"
      }
    },
    {
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#98bfff"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#6fcad0"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#6fcad0"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#6fd560",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.class",
        "support.type.primitive",
        "storage.type.type"
      ],
      "settings": {
        "foreground": "#65d590"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#e4b53f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage"
      ],
      "settings": {
        "foreground": "#e4b53f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#90918a"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#e8e4b1"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#e8e4b1",
        "background": "#622a1f"
      }
    },
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#e4b53f",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#c6a2fe",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.tag",
        "entity.name.tag",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#a0d13a",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#65d590"
      }
    },
    {
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#6fd560",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#ff7f4f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function.lisp",
      "settings": {
        "foreground": "#6fd560",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.lisp",
      "settings": {
        "foreground": "#e4b53f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language.lisp",
      "settings": {
        "foreground": "#98bfff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type.function.lisp",
      "settings": {
        "foreground": "#a0d13a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Line number active",
      "scope": "editorLineNumber.activeForeground",
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    }
  ]
}
