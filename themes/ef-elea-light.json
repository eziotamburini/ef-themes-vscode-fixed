{
  "name": "Ef-Elea-Light",
  "$schema": "vscode://schemas/color-theme",
  "type": "light",
  "colors": {
    "activityBar.background": "#edf5e2",
    "activityBar.foreground": "#221321",
    "activityBar.inactiveForeground": "#67647099",
    "activityBarBadge.background": "#a5c67f",
    "activityBarBadge.foreground": "#142810",
    "badge.background": "#a5c67f",
    "badge.foreground": "#142810",
    "breadcrumb.activeSelectionForeground": "#00601f",
    "breadcrumb.background": "#edf5e2",
    "breadcrumb.focusForeground": "#00601f",
    "breadcrumb.foreground": "#676470",
    "breadcrumbPicker.background": "#d0d7ca",
    "button.background": "#a5c67f",
    "button.foreground": "#142810",
    "button.hoverBackground": "#00704718",
    "checkbox.border": "#a5aaaf",
    "debugToolBar.background": "#edf5e2",
    "diffEditor.insertedTextBackground": "#c7e8ba33",
    "dropdown.background": "#edf5e2",
    "dropdown.border": "#d0d7ca",
    "editor.background": "#edf5e2",
    "editor.foreground": "#221321",
    "editor.findMatchHighlightBackground": "#efbf0033",
    "editor.lineHighlightBackground": "#d0e7c4",
    "editor.selectionBackground": "#d9d2ef",
    "editor.selectionForeground": "#000000",
    "editor.inactiveSelectionBackground": "#d9d2ef80",
    "editor.selectionHighlightBackground": "#d9d2ef40",
    "editorCursor.foreground": "#770080",
    "editorCursor.background": "#edf5e2",
    "editorError.foreground": "#d00000",
    "editorGroup.border": "#d0d7ca",
    "editorGroupHeader.tabsBackground": "#e8eddc",
    "editorGroupHeader.tabsBorder": "#d0d7ca",
    "editorHoverWidget.background": "#d0d7ca",
    "editorHoverWidget.border": "#a5aaaf",
    "editorIndentGuide.background": "#e3e9d6",
    "editorLineNumber.foreground": "#676470",
    "editorLineNumber.activeForeground": "#000000",
    "editorLink.activeForeground": "#00601f",
    "editorRuler.foreground": "#a5aaaf",
    "editorSuggestWidget.background": "#d0d7ca",
    "editorSuggestWidget.border": "#a5c67f",
    "editorSuggestWidget.selectedBackground": "#e3e9d6",
    "editorSuggestWidget.selectedForeground": "#221321",
    "editorWhitespace.foreground": "#a5aaaf",
    "editorWidget.background": "#d0d7ca",
    "extensionButton.prominentBackground": "#a5c67f33",
    "gitDecoration.addedResourceForeground": "#005000",
    "gitDecoration.modifiedResourceForeground": "#553d00",
    "gitDecoration.untrackedResourceForeground": "#22132188",
    "input.background": "#b0b7aa",
    "input.border": "#a5c67f",
    "input.foreground": "#221321",
    "inputOption.activeBorder": "#00601f",
    "list.activeSelectionBackground": "#b0b7aa",
    "list.activeSelectionForeground": "#221321",
    "list.dropBackground": "#d9d2ef",
    "list.errorForeground": "#c3303a",
    "list.focusBackground": "#b0b7aa",
    "list.highlightForeground": "#00601f",
    "list.hoverBackground": "#e3e9d6",
    "list.inactiveSelectionBackground": "#e8eddc",
    "list.inactiveSelectionForeground": "#221321",
    "mergeEditor.change.word.background": "#e3e9d6",
    "mergeEditor.conflict.handled.minimapOverViewRuler": "#00601f",
    "mergeEditor.conflict.unhandled.minimapOverViewRuler": "#1f70af",
    "notifications.background": "#a5c67f",
    "notifications.foreground": "#142810",
    "notifications.border": "#a5c67f",
    "peekView.border": "#00601f",
    "peekViewEditor.background": "#e3e9d6",
    "peekViewEditor.matchHighlightBackground": "#d0e7c4",
    "peekViewResult.background": "#d0d7ca",
    "peekViewTitle.background": "#e3e9d6",
    "scrollbarSlider.activeBackground": "#b0b7aabb",
    "scrollbarSlider.background": "#e3e9d699",
    "scrollbarSlider.hoverBackground": "#d0d7cacc",
    "sideBar.background": "#edf5e2",
    "sideBar.foreground": "#221321",
    "sideBarSectionHeader.background": "#e3e9d6",
    "sideBarSectionHeader.foreground": "#221321",
    "sideBarTitle.foreground": "#00601f",
    "statusBar.background": "#a5c67f",
    "statusBar.debuggingBackground": "#00601f",
    "statusBar.foreground": "#142810",
    "statusBar.noFolderBackground": "#a5c67f",
    "statusBarItem.hoverBackground": "#b0b7aa99",
    "tab.activeBackground": "#edf5e2",
    "tab.activeBorder": "#00601f",
    "tab.border": "#e8eddc",
    "tab.hoverBackground": "#e3e9d6",
    "tab.inactiveBackground": "#e8eddc",
    "tab.inactiveForeground": "#676470",
    "titleBar.activeBackground": "#edf5e2",
    "titleBar.activeForeground": "#221321",
    "titleBar.inactiveBackground": "#edf5e2",
    "titleBar.inactiveForeground": "#676470",
    "tree.indentGuidesStroke": "#a5aaaf",
    "terminal.ansiWhite": "#221321",
    "terminal.ansiBlack": "#e3e9d6",
    "terminal.ansiBlue": "#375cc6",
    "terminal.ansiCyan": "#3f6faf",
    "terminal.ansiGreen": "#00601f",
    "terminal.ansiMagenta": "#80308f",
    "terminal.ansiRed": "#c3303a",
    "terminal.ansiYellow": "#9a501f",
    "terminal.ansiBrightWhite": "#676470",
    "terminal.ansiBrightBlack": "#b0b7aa",
    "terminal.ansiBrightBlue": "#444fcf",
    "terminal.ansiBrightCyan": "#00677f",
    "terminal.ansiBrightGreen": "#007047",
    "terminal.ansiBrightMagenta": "#5032aa",
    "terminal.ansiBrightRed": "#b02440",
    "terminal.ansiBrightYellow": "#88541f"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7f4f4a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#007047"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#a5aaaf"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#5032aa"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#5032aa"
      }
    },
    {
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#5032aa"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#80308f"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#80308f"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#355500",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.class",
        "support.type.primitive",
        "storage.type.type"
      ],
      "settings": {
        "foreground": "#162f8f"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier.async"
      ],
      "settings": {
        "foreground": "#894852",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage"
      ],
      "settings": {
        "foreground": "#894852",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#676470"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#221321"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#221321",
        "background": "#f2d5b5"
      }
    },
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#00601f",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#6a4a9f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.tag",
        "entity.name.tag",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#375cc6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#162f8f"
      }
    },
    {
      "scope": "meta.function-call.generic",
      "settings": {
        "foreground": "#355500",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#9f356a",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function.lisp",
      "settings": {
        "foreground": "#355500",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.lisp",
      "settings": {
        "foreground": "#894852",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language.lisp",
      "settings": {
        "foreground": "#5032aa",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.type.function.lisp",
      "settings": {
        "foreground": "#007047",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "rx-escape",
      "settings": {
        "foreground": "#5032aa"
      }
    },
    {
      "scope": "rx-construct",
      "settings": {
        "foreground": "#c3303a"
      }
    },
    {
      "name": "Line number active",
      "scope": "editorLineNumber.activeForeground",
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    }
  ]
}
